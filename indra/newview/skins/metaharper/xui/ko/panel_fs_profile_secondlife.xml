<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 top="0"
 left="0"
 width="420"
 height="480"
 layout="topleft"
 follows="all"
 name="panel_profile"
 label="Profile">
    <string
     name="status_online">
온라인
    </string>
    <string
     name="status_offline">
오프라인
    </string>
    <string

<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toggleable_menu 
     name="menu_people_contact_sets_gear"
     left="0" bottom="0" visible="false"
     mouse_opaque="false">
  <menu_item_call
   label="프로필 보기"
   name="profile">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="profile"/>
     <menu_item_call.on_enable
      function="ContactSet.Enable"
      parameter="has_selection" />
  </menu_item_call>
  <menu_item_call
   label="IM..."
   name="im">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="im"/>
      <menu_item_call.on_enable
       function="ContactSet.Enable"
       parameter="has_selection" />
  </menu_item_call>
  <menu_item_call
   label="순간이동..."
   name="teleport">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="teleport"/>
     <menu_item_call.on_enable
      function="ContactSet.Enable"
      parameter="has_selection" />
  </menu_item_call>
  <menu_item_call
   label="별명 설정..."
   name="set_pseudonym">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="set_pseudonym"/>
     <menu_item_call.on_visible
      function="ContactSet.Enable"
      parameter="has_single_selection" />
  </menu_item_call>
  <menu_item_call
   label="별명 제거..."
   name="remove_pseudonym">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="remove_pseudonym"/>
     <menu_item_call.on_visible
      function="ContactSet.Enable"
      parameter="has_pseudonym" />
  </menu_item_call>
  <menu_item_call
   label="표시 이름 제거..."
   name="remove_displayname">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="remove_display_name"/>
     <menu_item_call.on_visible
      function="ContactSet.Enable"
      parameter="has_display_name" />
  </menu_item_call>
  <menu_item_call
   label="연락처 집합 구성..."
   name="set_config">
     <menu_item_call.on_click
      function="ContactSet.Action"
      parameter="set_config"/>
     <menu_item_call.on_enable
      function="ContactSet.Enable"
      parameter="has_mutable_set" />
  </menu_item_call>
</toggleable_menu>

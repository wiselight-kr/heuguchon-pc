<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toggleable_menu
 height="201"
 layout="topleft"
 mouse_opaque="false"
 name="place_overflow_menu"
 width="128">
    <menu_item_call
     label="SLurl 복사"
     layout="topleft"
     name="copy">
        <menu_item_call.on_click
         function="Places.OverflowMenu.Action"
         parameter="copy" />
    </menu_item_call>
     <menu_item_call
     label="랜드마크 생성"
     layout="topleft"
     name="landmark">
        <menu_item_call.on_click
         function="Places.OverflowMenu.Action"
         parameter="landmark" />
    </menu_item_call>
    <menu_item_call
     label="선택 생성"
     layout="topleft"
     name="pick">
        <menu_item_call.on_click
         function="Places.OverflowMenu.Action"
         parameter="pick" />
        <menu_item_call.on_enable
         function="Places.OverflowMenu.Enable"
         parameter="can_create_pick" />
    </menu_item_call>
</toggleable_menu>

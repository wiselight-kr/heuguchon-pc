<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 height="560"
 layout="topleft"
 name="floater_report_abuse"
 help_topic="floater_report_abuse"
 title="남용 신고"
 width="333">
    <floater.string
     name="Screenshot">
        스크린샷
    </floater.string>
    <texture_picker
     allow_no_texture="true"
     default_image_name="None"
     follows="left|top"
     height="150"
     layout="topleft"
     left="60"
     name="screenshot"
     top="20"
     width="220" />
     <button
     follows="top|left"
     halign="center"
     height="23"

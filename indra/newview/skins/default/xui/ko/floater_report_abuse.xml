     layout="topleft"
     tool_tip="스크린샷 새로고침"
     name="refresh_screenshot"
     left_pad="5"
     top="126"
     width="25"/>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left="10"
     name="reporter_title"
     top_pad="5"
     width="100">
        신고자:
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     layout="topleft"
     left_pad="5"
     name="reporter_field"
     top_delta="0"
     translate="false"
     use_ellipses="true"
     width="200">
        Loremipsum Dolorsitamut Longnamez
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left="10"
     name="sim_title"
     top_pad="2"
     width="100">
        지역:
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     layout="topleft"
     left_pad="5"
     name="sim_field"
     top_delta="0"
     use_ellipses="true"
     width="200">
        지역 이름
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left="10"
     name="pos_title"
     top_pad="2"
     width="100">
        위치:
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     layout="topleft"
     left_pad="5"
     name="pos_field"
     top_delta="0"
     width="200">
        {128.1, 128.1, 15.4}
    </text>
 <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     layout="topleft"
     left="10"
     name="select_object_label"
     top_pad="2"
     width="310">
        버튼을 클릭한 다음 남용 대상을 클릭합니다.:
    </text>
    <button
     height="32"
     layout="topleft"
     left_delta="0"
     name="pick_btn"
     image_overlay="Inv_Object"
     tool_tip="개체 선택기 - 개체를 이 신고의 제목으로 식별합니다"
     top_pad="0"
     width="32" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left="48"
     name="object_name_label"
     top_delta="0"
     width="80">
        개체:
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     layout="topleft"
     left_pad="6"
     name="object_name"
     top_delta="0"
     translate="false"
     parse_urls="false"
     use_ellipses="true"
     width="185">
        Consetetur Sadipscing
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left="48"
     name="owner_name_label"
     top_pad="0"
     width="80">
        소유주:
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     layout="topleft"
     left_pad="6"
     name="owner_name"
     top_delta="0"
     translate="false"
     use_ellipses="true"
     width="185">
        Hendrerit Vulputate Kamawashi Longname
    </text>
    <combo_box
     height="23"
     layout="topleft"
     left="10"
     name="category_combo"
     tool_tip="범주 - 이 신고를 가장 잘 설명하는 범주를 선택합니다."
     top_pad="5"
     width="313">
      <!-- Values can be populated from capability -->
        <combo_box.item
         label="카테고리 선택"
         name="Select_category"
         value="0" />
        <combo_box.item
         label="나이 &gt; 나이 재생"
         name="Age__Age_play"
         value="31" />       
        <combo_box.item
         label="폭행 &gt; 사격, 밀침, 혹은 안전한 지역에 다른 거주자를 밀치기"
         name="Assault__Safe_area"
         value="35" />        
        <combo_box.item
         label="공개 &gt; 실제 월드 정보"
         name="Disclosure__Real_world_information"
         value="39" />     
        <combo_box.item
         label="평화 방해 &gt; 과도한 개체 스크립트"
         name="Disturbing_the_peace__Excessive_scripted_objects"
         value="43" />
        <combo_box.item
         label="평화를 어지럽힘 &gt; 개체 쓰레기 버림"
         name="Disturbing_the_peace__Object_littering"
         value="44" />
        <combo_box.item
         label="평화 방해 &gt; 반복 스팸"
         name="Disturbing_the_peace__Repetitive_spam"
         value="45" />        
        <combo_box.item
         label="사기 &gt; L$ 혹은 USD $"
         name="Fraud__US$"
         value="50" />       
        <combo_box.item
         label="괴롭힘 &gt; 중단을 의도한 대상 행동"
         name="Harassment__Solicting_inciting_others_to_violate_ToS"
         value="55" />       
        <combo_box.item
         label="부적절 &gt; 광범위하게 모욕적인 내용 또는 행위"
         name="Indecency__Broadly_offensive_content_or_conduct"
         value="57" />
        <combo_box.item
         label="부적절 &gt; 부적절한 아바타 이름"
         name="Indecency__Inappropriate_avatar_name"
         value="59" />
        <combo_box.item
         label="부적절 &gt; 지역 등급에 부적절한 내용 또는 행위"
         name="Indecency__Mature_content_in_PG_region"
         value="60" />       
        <combo_box.item
         label="편협함"
         name="Intolerance"
         value="61" />        
        <combo_box.item
         label="랜드 &gt; 침해 &gt; 대체나 텍스쳐"
         name="Land__Encroachment__Objects_textures"
         value="63" />        
        <combo_box.item
         label="스킬 게임 정책 위반"
         name="Wagering_gambling"
         value="67" />       
    </combo_box>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="14"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left_delta="0"
     name="abuser_name_title"
     top_pad="6"
     width="313">
        남용자 이름:
    </text>
    <line_editor
     border_style="line"
     border_thickness="1"
     follows="left|top"
     height="23"
     layout="topleft"
     left_delta="0"
     name="abuser_name_edit"
     top_pad="0"
     width="195" />
    <button
     height="23"
     label="선택"
     layout="topleft"
     left_pad="5"
     name="select_abuser"
     tool_tip="목록에서 악용자 이름 선택"
     top_delta="0"
     width="100" />
<!--    <check_box
     height="23"
     label="Don&apos;t know abuser&apos;s name"
     layout="topleft"
     left="10"
     name="omit_abuser_name"
     tool_tip="Check this if you are unable to provide the abuser&apos;s name"
     top_pad="2"
     visible="false"
     width="250" />-->
    <text
     type="string"
     length="1"
     follows="left|top"
     height="14"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left="10"
     name="abuser_name_title2"
     top_pad="2"
     width="313">
        남용 위치:
    </text>
    <line_editor
     border_style="line"
     border_thickness="1"
     follows="left|top"
     height="23"
     layout="topleft"
     left="10"
     max_length_bytes="256"
     name="abuse_location_edit"
     top_pad="0"
     width="313" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="16"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left_delta="0"
     name="sum_title"
     top_pad="2"
     width="313">
        요약:
    </text>
    <line_editor
     border_style="line"
     border_thickness="1"
     follows="left|top"
     height="23"
     layout="topleft"
     left_delta="0"
     max_length="64"
     name="summary_edit"
     top_pad="0"
     width="313" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="14"
     font.name="SansSerif"
     font.style="BOLD"
     layout="topleft"
     left_delta="0"
     name="dscr_title"
     top_pad="6"
     width="50">
        세부사항:
    </text>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="22"
     layout="topleft"
     name="bug_aviso"
     left_pad="10"
     word_wrap="true"
     top_pad="-13"
     width="270">
        가능한 구체적으로 말씀해 주십시오.
    </text>
    <text_editor
     follows="left|top"
     height="75"
     layout="topleft"
     left="10"
     max_length="800"
     mouse_opaque="false"
     name="details_edit"
     width="313"
     top_pad="-6"
     word_wrap="true" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="30"
     layout="topleft"
     left="10"
     font.name="SansSerifSmall"
     name="incomplete_title"
     top_pad="2"
     word_wrap="true"
     width="313">
        * 가능한 구체적으로 말씀해 주십시오. 불완전한 신고는 조사되지 않습니다.
    </text>
     <button
     left="80"
     follows="left|top"
     height="23"
     top="532"
     label="남용 신고"
     label_selected="Report Abuse"
     layout="topleft"
     name="send_btn"
     width="140" />
     <button
     follows="left|top"
     height="23"
     label="취소"
     label_selected="Cancel"
     layout="topleft"
     name="cancel_btn"
     left_pad="5"
     width="100" />
</floater>

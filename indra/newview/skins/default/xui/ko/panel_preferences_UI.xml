<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 layout="topleft"
 left="1"
 top="1"
 width="540"
 label="UI-환경설정"
 name="Display panel">
<tab_container
 label="Firestorm 환경설정"
 layout="topleft"
 follows="all"
 top="5"
 bottom="-1"
 left="1"
 right="-1"
 name="tabs"
 tab_min_width="50"
 tab_position="top" >

    <!--3D World-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="3D 월드"
     name="UI Prefs Panel" >

        <text
         type="string"
         left="10"
         top_pad="8"
         length="1"
         follows="left|top"
         height="12"
         layout="topleft"
         name="HoverTipText"
         width="100">
            호버 팁:
        </text>
        <check_box
         control_name="ShowHoverTips"
         height="16"
         initial_value="true"
         label="호버 팁 보기"
         layout="topleft"
         left_delta="10"
         name="ShowHoverTips"
         tool_tip="아바타 등에 호버 팁을 표시하려면 이 옵션을 설정합니다(Ctrl+SHIFT+T)"
         top_pad="7"
         width="256" />
        <check_box
         enabled_control="ShowHoverTips"
         control_name="ShowAllObjectHoverTip"
         height="16"
         initial_value="true"
         label="모든 개체에"
         tool_tip="내부 개체에 대한 호버 팁을 표시하려면 이 옵션을 설정합니다"
         layout="topleft"
         left_delta="10"
         name="ShowAllHoverTips"
         top_pad="2"
         width="256" />
        <check_box
         enabled_control="ShowHoverTips"
         control_name="ShowLandHoverTip"
         height="16"
         initial_value="true"
         label="땅에"
         tool_tip="육지에 호버 팁을 표시하려면 이 옵션을 설정합니다 "
         layout="topleft"
         name="ShowLandHoverTips"
         top_pad="2"
         width="256" />
		<check_box
		enabled_control="ShowHoverTips"
		control_name="FSAdvancedTooltips"
		name="FSAdvancedTooltips"
		label="호버팁으로 확장 정보 표시"
		layout="topleft"
		top_pad="5"
		left_delta="-10"
		height="16"
		width="350" />
        <check_box
		enabled_control="ShowHoverTips"
		control_name="FSShowGroupTitleInTooltip"
		name="FSShowGroupTitleInTooltip"
		label="호스트 팁으로 아바타 그룹 제목 표시"
		layout="topleft"
		top_pad="2"
		height="16"
		width="350" />

        <text
         type="string"
         length="1"
         follows="left|top"
         height="12"
         layout="topleft"
         left="265"
         name="tooltip_textbox"
         top="8"
         width="200">
           호버팁 트리거 지연:
        </text>
		
        <slider
         control_name="ToolTipDelay"
         decimal_digits="3"
         follows="left|top"
         height="16"
         increment="0.025"
         initial_value="0.7"
         label="툴 팁:"
         label_width="50"
         layout="topleft"
         max_val="6.0"
         min_val="0"
         name="ToolTipDelay_slider"
         tool_tip="버튼과 같은 사용자 인터페이스 요소에 대한 툴팁이 표시된 후 지연 시간(기본값: 0.7)"
         top_pad="7"
         left="275"
         width="200" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left_pad="1"
         name="ToolTipDelay_text"
         top_delta="0"
         width="80">
            초
        </text>
		
        <slider
         enabled_control="ShowHoverTips"
         control_name="AvatarInspectorTooltipDelay"
         decimal_digits="3"
         follows="left|top"
         height="17"
         increment="0.025"
         initial_value="0.35"
         label="아바타:"
         label_width="50"
         layout="topleft"
         left="275"
         max_val="6.0"
         min_val="0"
         name="AvatarInspectorTooltipDelay_slider"
         tool_tip="아바타에 대한 대기 팁이 표시된 후 지연 시간(기본값: 0.35)"
         top_pad="2"
         width="200" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left_pad="1"
         name="AvatarInspectorTooltipDelay_text"
         top_delta="0"
         width="80">
            초
        </text>
		
        <slider
         enabled_control="ShowHoverTips"
         control_name="ObjectInspectorTooltipDelay"
         decimal_digits="3"
         follows="left|top"
         height="17"
         increment="0.025"
         initial_value="0.35"
         label="개체:"
         label_width="50"
         layout="topleft"
         left="275"
         max_val="6.0"
         min_val="0"
         name="ObjectInspectorTooltipDelay_slider"
         tool_tip="개체에 대한 대기 팁이 표시된 후 지연 시간(기본값: 0.35)"
         top_pad="2"
         width="200" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left_pad="1"
         name="ObjectInspectorTooltipDelay_text"
         top_delta="0"
         width="80">
            초
        </text>
        <slider
         control_name="FSHudTextFadeDistance"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.25"
         label="부동 텍스트 페이드 거리(m):"
         tool_tip="부동 텍스트가 페이드되기 시작하는 거리를 설정합니다."
         label_width="200"
         layout="topleft"
         left="10"
         min_val="0"
         max_val="64"
         name="FSHudTextFadeDistance"
         top="130"
         width="481" />
        <slider
         control_name="FSHudTextFadeRange"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.25"
         label="부동 텍스트 페이드 범위(m):"
         tool_tip="수상 텍스트 완전히 보이는에서 완전히 보이지 않는 희미하는 범위를 설정합니다."
         label_width="200"
         layout="topleft"
         left="10"
         min_val="0.25"
         max_val="32"
         name="FSHudTextFadeRange"
         top_pad="2"
         width="474" />
    </panel>

    <!--2D Overlay-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="2D 오버레이"
     name="ui-2d-overlay" >
        <text
         type="string"
         length="1"
         follows="left|top"
         height="12"
         layout="topleft"
         left="10"
         name="UI Size:"
         top_pad="8"
         width="80">
            UI 스케일링:
        </text>
        <slider
         control_name="UIScaleFactor"
         decimal_digits="2"
         follows="left|top"
         height="17"
         increment="0.025"
         initial_value="1"
         layout="topleft"
         left_pad="0"
         max_val="2.0"
         min_val="0.75"
         name="ui_scale_slider"
         top_pad="-14"
         width="390" />

        <text
         type="string"
         length="1"
         follows="left|top"
         height="12"
         layout="topleft"
         left="10"
         name="HUD Size:"
         top_pad="5"
         width="80">
            HUD 스케일링:
        </text>
        <slider
         control_name="HUDScaleFactor"
         decimal_digits="2"
         follows="left|top"
         height="17"
         increment="0.1"
         initial_value="1"
         layout="topleft"
         left_pad="0"
         max_val="2.0"
         min_val="1.0"
         name="ui_scale_slider"
         top_pad="-14"
         width="390" />

      <text
       type="string"
       left="10"
       top_pad="15"
       width="140"
       follows="left|top"
       height="16"
       layout="topleft"
       name="ScriptDialogsPerObjectLabel">
        개체별 스크립트 대화:
      </text>
      <combo_box
       control_name="ScriptDialogLimitations"
       height="23"
       layout="topleft"
       left_pad="10"
       top_delta="-5"
       name="ScriptDialogPerObjectDropdown"
       width="180">
        <combo_box.item
         label="개체 당 하나"
         name="ScriptDialogOption_0"
         value="0"/>
        <combo_box.item
         label="채널 당 하나"
         name="ScriptDialogOption_1"
         value="1"/>
        <combo_box.item
         label="첨부파일의 경우 채널 당 하나"
         name="ScriptDialogOption_2"
         value="2"/>
        <combo_box.item
         label="HUD의 경우 채널 당 하나"
         name="ScriptDialogOption_3"
         value="3"/>
        <combo_box.item
         label="HUD에 대한 제한 없음"
         name="ScriptDialogOption_4"
         value="4"/>
        <combo_box.item
         label="제한 없음"
         name="ScriptDialogOption_5"
         value="5"/>
      </combo_box>
        <check_box
         control_name="FSRemoveScriptBlockButton"
         height="16"
         label="제거 &quot;블록&quot; 버튼"
         layout="topleft"
         left_pad="5"
         name="FSRemoveScriptBlockButton"
         top_delta="4"
         width="256" />

      <text
       type="string"
       left="10"
       top_pad="9"
       width="140"
       follows="left|top"
       height="16"
       layout="topleft"
       name="ScriptDialogsPositionLabel">
        스크립트 대화 상자 위치:
      </text>

        <combo_box
         control_name="ScriptDialogsPosition"
         height="23"
         layout="topleft"
         left_pad="10"
         top_delta="-5"
         name="ScriptDialogsPositionDropdown"
         tool_tip="화면에서 스크립트 대화 상자(메뉴)를 표시할 위치 선택."
         width="180">

        <combo_box.item
         label="치클렛에 도킹"
         name="ScriptDialogPositionOption_1"
         value="1"/>
        <combo_box.item
         label="왼쪽 위"
         name="ScriptDialogPositionOption_2"
         value="2"/>
        <combo_box.item
         label="오른쪽 위"
         name="ScriptDialogPositionOption_3"
         value="3"/>
        <combo_box.item
         label="왼쪽 아래"
         name="ScriptDialogPositionOption_4"
         value="4"/>
        <combo_box.item
         label="오른쪽 아래"
         name="ScriptDialogPositionOption_5"
         value="5"/>

        </combo_box>
        <check_box
         control_name="FSAnimatedScriptDialogs"
         height="16"
         initial_value="true"
         label="애니메이션 스크립트 대화 상자"
         layout="topleft"
         left_pad="5"
         name="FSAnimatedScriptDialogs"
         tool_tip="열 때 스크립트 대화 상자가 활성화되도록 하려면 이 옵션을 설정합니다(클래식 V1.x 동작). 대화 상자가 위에서 아래로 열릴 때만 작동합니다."
         top_delta="4"
         width="256" />

        <slider
         control_name="FSRowsPerScriptDialog"
         decimal_digits="0"
         follows="left|top"
         height="16"
         increment="1"
         initial_value="20"
         label="스크립트당 표시되는 행 수 대화 상자:"
         label_width="160"
         layout="topleft"
         max_val="50"
         min_val="2"
         name="FSRowsPerScriptDialog"
         tool_tip="동시에 표시되는 스크립트 대화 상자의 최대 행 수."
         top_pad="9"
         left="10"
         width="400" />

      <check_box
       control_name="UsePieMenu"
       height="16"
       label="원형 메뉴 사용"
       layout="topleft"
       left="5"
       name="UsePieMenu"
       tool_tip="랜드, 아바타, 객체 또는 첨부 파일을 마우스 오른쪽 버튼으로 클릭할 때 직사각형 컨텍스트 메뉴 대신 클래식 V1 링 메뉴 사."
       top_pad="12"
       width="256" />
      <check_box
       control_name="PieMenuPopupFontEffect"
       enabled_control="UsePieMenu"
       height="16"
       label="텍스트 애니메이션 사용"
       layout="topleft"
       left="15"
       name="PieMenuPopupFontEffect"
       tool_tip="원형 메뉴가 열릴 때 애니메이션에 포함된 원형 메뉴 내의 텍스트를 활성화"
       top_pad="3"
       width="256" />
      <check_box
       control_name="PieMenuOuterRingShade"
       enabled_control="UsePieMenu"
       height="16"
       label="외부 링 셰이드를 활성화"
       layout="topleft"
       left="15"
       name="PieMenuOuterRingShade"
       tool_tip="하위 메뉴를 시각화하는 데 도움이 되는 추가 외부 링 음영 사용."
       top_pad="3"
       width="256" />

        <text
         follows="left|top"
         height="16"
         layout="topleft"
         left="10"
         name="ShowGroupNoticesTopRightLabel"
         top_pad="8"
         type="string"
         width="210">
           그룹 알림 및 치클렛 위치:
        </text>

        <radio_group
         control_name="ShowGroupNoticesTopRight"
         height="16"
         initial_value="1"
         layout="topleft"
         left_pad="5"
         top_delta="-1"
         name="ShowGroupNoticesTopRight"
         tool_tip="화면에 그룹 알림 및 키클릿 아이콘을 표시할 위치 선택."
         width="200">
        <radio_item
         height="16"
         label="오른쪽 아래"
         layout="topleft"
         left="0"
         name="radio"
         top_pad="2"
         value="0"
         width="100" />
        <radio_item
         height="16"
         label="오른쪽 위"
         layout="topleft"
         left_pad="0"
         name="radio1"
         value="1"
         width="100" />
        </radio_group>
        <text
         follows="left|top"
         height="16"
         layout="topleft"
         left_pad="2"
         name="GroupTopRightRequiresRestart"
         text_color="White_50"
         top_pad="-14"
         type="string"
         width="128">
           (재시작 요청)
        </text>

        <check_box
         control_name="FSDisableIMChiclets"
         height="16"
         label="그룹 및 IM 채팅 치클 숨기기"
         layout="topleft"
         left="5"
         name="FSDisableIMChiclets"
         tool_tip="이 옵션을 선택하면 Firestorm에서 그룹/IM 채팅 치클렛이 표시되지 않습니다(알림 봉투 및 IM 합계는 화면에 남습니다)."
         top_pad="6"
         width="256" />
        <check_box
         control_name="FSLegacyNotificationWell"
         height="16"
         initial_value="true"
         label="알림 창에 레거시 목록 레이아웃 사용"
         layout="topleft"
         left="5"
         name="FSLegacyNotificationWell"
         tool_tip="이 옵션을 선택하면 Firestorm은 레거시 목록 보기를 사용하여 저장된 알림을 표시합니다."
         top_pad="3"
         width="265" />
        <text
         type="string"
         left_pad="10"
         top_pad="-15"
         width="128"
         follows="left|top"
         height="12"
         layout="topleft"
         text_color="White_50"
         name="FSLegacyNotificationWellRequiresRestart">
            (재시작 요청)
        </text>
        <check_box
         control_name="FSLegacyNotificationWellAutoResize"
         height="16"
         initial_value="true"
         label="대화 및 레거시 알림 목록 자동 크기 조정"
         layout="topleft"
         left="5"
         name="FSLegacyNotificationWellAutoResize"
         tool_tip="이 옵션을 선택하면 항목이 추가 또는 제거될 때 Firestorm은 항상 대화 및 레거시 알림 목록의 크기를 자동으로 조정합니다."
         top_pad="3"
         width="450" />
        <check_box
         control_name="FSRemapLinuxShortcuts"
         height="16"
         label="Linux에서 이미 사용 중인 재맵 바로 가기(Linux만 해당, 다시 시작해야 함)"
         layout="topleft"
         left="5"
         name="FSRemapLinuxShortcuts"
         tool_tip="사용하도록 설정한 경우 리눅스 운영 체제에서 이미 사용하고 있는 바로 가기(예: CTRL-ALT-Fx)가 다시 매핑됩니다."
         top_pad="5"
         width="256" />
        <check_box
         control_name="FSStatisticsNoFocus"
         height="16"
         label="통계 창의 포커스 방지"
         layout="topleft"
         left="5"
         name="FSStatisticsNoFocus"
         tool_tip="활성화된 경우 다른 창이 닫혔을 때 통계 창에 포커스가 표시되지 않습니다(활성화된 경우 항목의 그래프 레이아웃 변경과 같은 작업은 계속 가능합니다)."
         top_pad="3"
         width="256" />

        <slider
         control_name="FlashCount"
         decimal_digits="0"
         follows="left|top"
         height="16"
         increment="1"
         label="IM 탭이 깜박이는 횟수:"
         label_width="200"
         layout="topleft"
         left="10"
         min_val="0"
         max_val="16"
         name="ButtonsFlashCount"
         top_pad="14"
         width="471" />
        <slider
         control_name="FlashPeriod"
         decimal_digits="2"
         follows="left|top"
         height="16"
         increment="0.25"
         label="IM 탭 플래시 기간:"
         tool_tip="IM 탭을 한 번 깜박이는 데 걸리는 시간(초) - IM 알림에 사용"
         label_width="200"
         layout="topleft"
         left="10"
         min_val="0.25"
         max_val="4"
         name="ButtonFlashRate"
         top_pad="2"
         width="467" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left="490"
         name="ButtonFlashRateText"
         top_delta="0"
         width="50">
            초
        </text>

    </panel>	

    <!--Top Bars-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="상단 막대"
     name="UI-interface-top-bars" >

		<text
		type="string"
		length="1"
		follows="left|top"
		height="16"
		layout="topleft"
		name="statusbar_textbox"
		left="10"
		top_pad="8"
		width="200">
		상태 막대:
		</text>

		<check_box
		 control_name="ShowNetStats"
		 height="16"
		 tool_tip="상단 메뉴에 트래픽 표시기 표시"
		 label="화면의 오른쪽 상단 모서리에 트래픽 표시기 표시"
		 layout="topleft"
		 name="ShowNetStats"
		 top_pad="8"
		 left="10"
		 width="250" />
		<check_box
		 enabled_control="ShowNetStats"
		 top_pad="4"
		 follows="left|top"
		 height="16"
		 label="트래픽 표시기에 초당 레거시 기간 평균 표시 사용"
		 name="FSStatbarLegacyMeanPerSec"
		 width="270"
		 left="20"
		 control_name="FSStatbarLegacyMeanPerSec"
		 tool_tip="활성화된 경우 Firestorm은 트래픽 표시기에 현재 관측 값 대신 초당 기간 평균 값을 표시하는 기존 방법을 사용합니다"/>
		<check_box
		 enabled_control="ShowNetStats"
		 top_pad="4"

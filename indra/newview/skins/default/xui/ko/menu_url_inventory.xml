<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="Url Popup">
    <menu_item_call
     label="인벤토리 아이템 표시"
     layout="topleft"
     name="show_item">
        <menu_item_call.on_click
         function="Url.Execute" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft" />
    <menu_item_call
     label="클립보드에 이름 복사"
     layout="topleft"
     name="url_copy_label">
        <menu_item_call.on_click
         function="Url.CopyLabel" />
    </menu_item_call>
    <menu_item_call
     label="SLurl을 클립보드에 복사"
     layout="topleft"
     name="url_copy">
        <menu_item_call.on_click
         function="Url.CopyUrl" />
    </menu_item_call>
</context_menu>

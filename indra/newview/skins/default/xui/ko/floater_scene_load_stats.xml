<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater legacy_header_height="18"
         can_resize="true"
         height="400"
         layout="topleft"
         name="Scene Load Statistics"
         save_rect="true"
         save_visibility="true"
         title="장면 로드 통계"
         min_width="250"
         width="400">
    <scroll_container follows="top|left|bottom|right"
                      bottom="400"
                      layout="topleft"
                      left="0"
                      name="statistics_scroll"
                      reserve_scroll_corner="true"
                      top="20"
                      width="395">
      <container_view follows="top|left|bottom|right"
                      height="378"
                      layout="topleft"
                      left="2"
                      name="statistics_view"
                      top="20"
                      width="380" >
<!--Basic Section-->
		  <stat_view name="basic"
                 label="기본"
                 show_label="true"
                 setting="OpenDebugStatBasic">
        <stat_bar name="frame difference"
                  label="프레임 픽셀 차이"
                  orientation="horizontal"
                  unit_label="%"
                  stat="FramePixelDifference"
                  bar_max="100"
                  tick_spacing="10"
                  unit_scale="100"
                  precision="0"/>
        <stat_bar name="bandwidth"
                  label="수신된 UDP 데이터"
                  orientation="horizontal"
                  stat="activemessagedatareceived"
                  bar_max="5000"
                  tick_spacing="500"
                  precision="0"/>
			  <stat_bar name="packet_loss"
                  label="패킷 손실"
                  orientation="horizontal"
                  unit_label=" %"
                  stat="packetslostpercentstat"
                  bar_max="5"
                  tick_spacing="0.5"
                  precision="3"
                  show_bar="false"
                  show_mean="true"/>
		  </stat_view>
<!--Advanced Section-->
      <stat_view name="advanced"
                 label="고급"
                 show_label="true"
                 setting="OpenDebugStatAdvanced">
			  <stat_view name="render"
                   label="렌더"
                   show_label="true"
                   setting="OpenDebugStatRender">
          <stat_bar name="objs"
                    label="종합 개체"
                    orientation="horizontal"
                    unit_label=""
                    stat="numobjectsstat"
                    bar_max="50000"
                    tick_spacing="5000"
                    precision="0"
                    show_bar="false"/>
          <stat_bar name="newobjs"
                    label="새로운 개체"
                    orientation="horizontal"
                    unit_label="/sec"
                    stat="numnewobjectsstat"
                    bar_max="2000"
                    tick_spacing="200"
                    show_bar="false"/>
          <stat_bar name="object_cache_hits"
                    label="개체 캐시 적중률"
                    orientation="horizontal"
                    stat="object_cache_hits"
                    bar_max="100"
                    unit_label="%"
                    tick_spacing="20"
                    show_history="true"
                    show_bar="false"/>
			  </stat_view>
<!--Texture Stats-->
			  <stat_view name="texture"
                   label="텍스쳐"
                   show_label="true">
			    <stat_bar name="texture_cache_hits"
                    label="캐시 적중률"
                    orientation="horizontal"
                    stat="texture_cache_hits"
                    bar_max="100.f"
                    unit_label="%"
                    tick_spacing="20"
                    show_history="true"
                    show_bar="false"/>
          <stat_bar name="texture_cache_read_latency"
                    label="캐시 읽기 지연 시간"
                    orientation="horizontal"
                    unit_label="msec"
                    stat="texture_cache_read_latency"
                    bar_max="1000.f"
                    tick_spacing="100"
                    show_history="true"
                    show_bar="false"/>
          <stat_bar name="numimagesstat"
                    label="개수"
                    orientation="horizontal"
                    stat="numimagesstat" 
                    bar_max="8000.f" 
                    tick_spacing="2000.f"
                    show_bar="false"/>
			    <stat_bar name="numrawimagesstat"
                    label="원 개수"
                    orientation="horizontal"
                    stat="numrawimagesstat"
                    bar_max="8000.f" 
                    tick_spacing="2000.f"
                    show_bar="false"/>
			  </stat_view>
<!--Network Stats-->
			  <stat_view name="network"
                   label="네트워크"
                   show_label="true"
                   setting="OpenDebugStatNet">
			    <stat_bar name="packetsinstat"
                    label="패킷 수신"
                    orientation="horizontal"
                    stat="packetsinstat"
                    unit_label="/sec" 
                    bar_max="1024.f" 
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="packetsoutstat"
                    label="패킷 발신"
                    orientation="horizontal"
                    stat="packetsoutstat"
                    unit_label="/sec"  
                    bar_max="1024.f" 
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="objectdatareceived"
                    label="개체"
                    orientation="horizontal"
                    stat="objectdatareceived"
                    unit_label="kbps"
                    bar_max="1024.f"
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="texturedatareceived"
                    label="텍스쳐"
                    orientation="horizontal"
                    stat="texturedatareceived"
                    unit_label="kbps"
                    bar_max="1024.f"
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="assetudpdatareceived"
                    label="재산"
                    orientation="horizontal"
                    stat="assetudpdatareceived"
                    unit_label="kbps"
                    bar_max="1024.f"
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="layersdatareceived"
                    label="계층"
                    orientation="horizontal"
                    stat="layersdatareceived"
                    unit_label="kbps"
                    bar_max="1024.f"
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="messagedatain"
                    label="실제 입력"
                    orientation="horizontal"
                    stat="messagedatain"
                    unit_label="kbps"
                    bar_max="1024.f"
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			    <stat_bar name="messagedataout"
                    label="실제 출력"
                    orientation="horizontal"
                    stat="messagedataout"
                    unit_label="kbps"
                    bar_max="1024.f"
                    tick_spacing="128.f"
                    precision="1"
                    show_bar="false"/>
			  </stat_view>
		  </stat_view>
<!--Sim Stats-->
		  <stat_view name="sim"
                 label="시뮬레이터"
                 show_label="true"
                 setting="OpenDebugStatSim">
			  <stat_bar name="simobjects"
                  label="개체"
                  orientation="horizontal"
                  stat="simobjects"
                  precision="0"
                  bar_max="30000.f" 
                  tick_spacing="5000.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="simactiveobjects"
                  label="개체 활성화"
                  orientation="horizontal"
                  stat="simactiveobjects"
                  precision="0"
                  bar_max="5000.f" 
                  tick_spacing="750.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="simactivescripts"
                  label="스크립트 활성화"
                  orientation="horizontal"
                  stat="simactivescripts"
                  precision="0"
                  bar_max="15000.f" 
                  tick_spacing="1875.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="siminpps"
                  label="패킷 수신"
                  orientation="horizontal"
                  stat="siminpps"
                  unit_label="pps"
                  precision="0"
                  bar_max="2000.f" 
                  tick_spacing="250.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="simoutpps"
                  label="패킷 발신"
                  orientation="horizontal"
                  stat="simoutpps"
                  unit_label="pps" 
                  precision="0"
                  bar_max="2000.f" 
                  tick_spacing="250.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="simpendingdownloads"
                  label="보류 중인 다운로드"
                  orientation="horizontal"
                  stat="simpendingdownloads"
                  precision="0"
                  bar_max="800.f" 
                  tick_spacing="100.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="simpendinguploads"
                  label="보류 중인 업로드"
                  orientation="horizontal"
                  stat="simpendinguploads"
                  precision="0"
                  bar_max="100.f" 
                  tick_spacing="25.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_bar name="simtotalunackedbytes"
                  label="언락된 총 바이트 수"
                  orientation="horizontal"
                  stat="simtotalunackedbytes"
                  unit_label="kb"
                  precision="1"
                  bar_max="100000.f" 
                  tick_spacing="25000.f"
                  show_bar="false"
                  show_mean="false"/>
			  <stat_view name="simperf"
                   label="시간 (ms)"
                   show_label="true">
			    <stat_bar name="simframemsec"
                    label="총 프레임 시간"
                    orientation="horizontal"
                    stat="simframemsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
			    <stat_bar name="simnetmsec"
                    label="넷 타임"
                    orientation="horizontal"
                    stat="simnetmsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
			    <stat_bar name="simsimphysicsmsec"
                    label="물리 시간"
                    orientation="horizontal"
                    stat="simsimphysicsmsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
			    <stat_bar name="simsimothermsec"
                    label="시뮬레이션 시간"
                    orientation="horizontal"
                    stat="simsimothermsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
			    <stat_bar name="simagentmsec"
                    label="요원 시간"
                    orientation="horizontal"
                    stat="simagentmsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
			    <stat_bar name="simimagesmsec"
                    label="이미지 시간"
                    orientation="horizontal"
                    stat="simimagesmsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
			    <stat_bar name="simscriptmsec"
                    label="스크립트 시간"
                    orientation="horizontal"
                    stat="simscriptmsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f" 
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
          <stat_bar name="simsparemsec"
                    label="여유 시간"
                    orientation="horizontal"
                    stat="simsparemsec"
                    unit_label="ms"
                    precision="3"
                    bar_max="40.f"
                    tick_spacing="10.f"
                    show_bar="false"
                    show_mean="false"/>
<!--2nd level time blocks under 'Details' second-->
          <stat_view name="timedetails"
                     label="시간 세부사항 (ms)"
                     show_label="true">
            <stat_bar name="simsimphysicsstepmsec"
                      label="  물리학 단계"
                      orientation="horizontal"
                      stat="simsimphysicsstepmsec"
                      unit_label="ms"
                      precision="3"
                      bar_max="40.f"
                      tick_spacing="10.f"
                      show_bar="false"
                      show_mean="false"/>
            <stat_bar name="simsimphysicsshapeupdatemsec"
                      label="  물리적 형상 업데이트"
                      orientation="horizontal"
                      stat="simsimphysicsshapeupdatemsec"
                      unit_label="ms"
                      precision="3"
                      bar_max="40.f"
                      tick_spacing="10.f"
                      show_bar="false"
                      show_mean="false"/>
            <stat_bar name="simsimphysicsothermsec"
                      label="  물리 기타"
                      orientation="horizontal"
                      stat="simsimphysicsothermsec"
                      unit_label="ms"
                      precision="3"
                      bar_max="40.f"
                      tick_spacing="10.f"
                      show_bar="false"
                      show_mean="false"/>
            <stat_bar name="simsleepmsec"
                      label="  수면 시간"
                      orientation="horizontal"
                      stat="simsleepmsec"
                      unit_label="ms"
                      precision="3"
                      bar_max="40.f"
                      tick_spacing="10.f"
                      show_bar="false"
                      show_mean="false"/>
            <stat_bar name="simpumpiomsec"
                      label="  IO 펌프"
                      orientation="horizontal"
                      stat="simpumpiomsec"
                      unit_label="ms"
                      precision="3"
                      bar_max="40.f"
                      tick_spacing="10.f"
                      show_bar="false"
                      show_mean="false"/>
          </stat_view>
			  </stat_view>
		  </stat_view>
    </container_view>
  </scroll_container>
</floater>

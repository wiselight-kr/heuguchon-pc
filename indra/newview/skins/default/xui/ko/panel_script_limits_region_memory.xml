<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="top|left"
 height="570"
 label="지역 메모리"
 layout="topleft"
 name="script_limits_region_memory_panel"
 top="0"
 left="0"
 width="620">
	<text
	 type="string"
	 length="1"
	 follows="left|top"
	 height="16"
	 layout="topleft"
	 left="10"
	 name="script_memory"
	 top_pad="24"
	 text_color="White"
	 width="620">
	   구획 스크립트 메모리
	</text>
	<text
	 type="string"
	 length="1"
	 follows="left|top"
	 height="16"
	 layout="topleft"
	 left="30"
	 name="parcels_listed"
	 top_delta="18"
	 visible="true"
	 width="620">
	 
	</text>
	<text
	 type="string"
	 length="1"
	 follows="left|top"
	 height="16"
	 layout="topleft"
	 left="30"
	 name="memory_used"
	 top_delta="18"
	 width="620">
	</text>
	
	<text
	 type="string"
	 length="1"
	 follows="left|top"
	 height="16"
	 layout="topleft"
	 left="30"
	 name="urls_used"
	 top_delta="18"
	 width="620">
	 
	</text>
	<text
     type="string"
     length="1"
     follows="left|top"
     height="20"
     layout="topleft"
     left="10"
     name="loading_text"
     top_delta="12"
     text_color="EmphasisColor"
     width="620">
        로딩중...
    </text>
    <scroll_list
     draw_heading="true"
     follows="all"
     height="409"
     layout="topleft"
     left_delta="0"
     multi_select="true"
     sort_column="0"
     sort_ascending="true"
     name="scripts_list"
     top_delta="16"
     width="600">
        <scroll_list.columns
         label="크기 (kb)"
         name="size"
         width="72" />
        <scroll_list.columns
         label="URLs"
         name="urls"
         width="48" />
        <scroll_list.columns
         label="개체 이름"
         name="name"
         width="100" />
        <scroll_list.columns
         label="개체 소유자"
         name="owner"
         width="100" />
        <scroll_list.columns
         label="구획"
         name="parcel"
         width="130" />
        <scroll_list.columns
         label="위치"
         name="location"
         width="80" />
    </scroll_list>
    <button
     follows="bottom|left"
     height="19"
     label="목록 새로고침"
     layout="bottomleft"
     left_pad="5"
     name="refresh_list_btn"
     top="34"
     left="10"
     width="100" />
    <button
     follows="bottom|left"
     height="19"
     label="반환"
	 visible="false"
     layout="bottomleft"
     name="return_btn"
     top="34"
     left_delta="390"
     width="100" />
    <button
     follows="bottom|left"
     height="19"
     label="하이라이트"
	 visible="false"
     layout="bottomleft"
     left_delta="105"
     name="highlight_btn"
     top="34"
     width="100" />
</panel>

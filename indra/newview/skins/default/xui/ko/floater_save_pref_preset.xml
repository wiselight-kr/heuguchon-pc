<?xml version="1.0" encoding="UTF-8"?>
<floater
 legacy_header_height="18"
 height="140"
 help_topic="floater_save_preset"
 layout="topleft"
 name="save_pref_preset"
 positioning="centered"
 save_rect="true"
 title="그래픽 사전 설정 저장"
 width="245">

    <text
     follows="top|left|right"
     height="32"
     layout="topleft"
     word_wrap="true"
     left="10"
     name="Preset"
     top="30"
     width="220">
     사전 설정의 이름을 입력하거나 기존 사전 설정 선택.
    </text>
    <combo_box
     follows="top|left"
     layout="topleft"
     left="10"
     name="preset_combo"
     top_delta="35"
     allow_text_entry="true"
     width="220"/>
    <button
     follows="top|left"
     height="23"
     label="저장"
     layout="topleft"
     top_delta="40"
     left="10"
     name="save"
     width="100"/>
    <button
     follows="bottom|right"
     height="23"
     label="취소"
     layout="topleft"
     left_pad="20"
     name="cancel"
     width="100"/>
</floater>

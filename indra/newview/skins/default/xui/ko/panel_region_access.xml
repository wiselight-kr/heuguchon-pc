<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="top|left"
 height="510"
 help_topic="panel_region_access_tab"
 label="접근"
 layout="topleft"
 left="0"
 name="Access"
 top="320"
 width="485">


<tab_container
 bottom="-10"
 follows="all"
 layout="topleft"
 left="3"
 name="tabs"
 tab_min_width="70"
 tab_position="top"
 top="10"
 halign="center"
 right="-5">
    
<!-- ============================= ESTATE MANAGERS tab ====================== -->
  <panel
    background_opaque="true"
    background_visible="true"
    bg_alpha_color="DkGray"
    bg_opaque_color="DkGray"
    bottom="-1"
    follows="all"
    label="자산 관리자"
    layout="topleft"
    left="0"
    help_topic="estate_managers_tab"
    name="estate_managers_panel"
    right="-1"
    top="0">
    <text
     type="string"
     length="1"
     bottom="34"
     follows="left|top"
     height="20"
     layout="topleft"
     name="estate_manager_label"
     top_pad="10"
     left="10"
     width="200">
        자산 관리자:
    </text>
    <!-- Estate Managers box -->
    <view_border
     bevel_style="none"
     follows="top|left"
     height="400"
     layout="topleft"
     left="10"
     top_pad="-5"
     width="453" />
    <name_list
     follows="left|top"
     height="400"
     layout="topleft"
     left_delta="0"
     multi_select="true"
     draw_heading="true"
     name="estate_manager_name_list"
     top_delta="0"
     width="453">
        <columns
         label="이름"
         name="name"
         width="453" />
    </name_list>
    <!-- Estate Managers buttons -->
    <button
     follows="left|top"
     height="23"
     label="추가..."
     layout="topleft"
     left="10"
     name="add_estate_manager_btn"
     top_pad="6"
     width="114" />
    <button
     follows="left|top"
     height="23"
     label="제거..."
     layout="topleft"
     name="remove_estate_manager_btn"
     left_pad="6"
     width="114" />      
  </panel>
        
        
<!-- ================================ ALLOWED tab =========================== -->
  <panel
    background_opaque="true"
    background_visible="true"
    bg_alpha_color="DkGray"
    bg_opaque_color="DkGray"
    bottom="-1"
    follows="all"
    label="허용"
    layout="topleft"
    left="0"
    help_topic="allowed_tab"
    name="allowed_panel"
    right="-1"
    top="0">
       <panel
        follows="left|top|right"
        height="27"
        label="상부_패널"
        layout="topleft"
        left="0"
        name="allowed_search_panel"
        right="-1"
        top="0">
           <filter_editor
            follows="left|top|right"
            height="23"
            layout="topleft"
            left="6"
            label="허용된 에이전트 검색"
            max_length_chars="300"
            name="allowed_search_input"
            top="4"
            width="395" />
           <button
            follows="left|top"
            height="23"
            label="복사"
            layout="topleft"
            left_pad="3"
            name="copy_allowed_list_btn"
            width="58" />
        </panel>
   <text
     type="string"
     length="1"
     bottom="34"
     follows="left|top"
     height="20"
     layout="topleft"
     name="allow_resident_label"
     top_pad="10"
     left="10"
     width="200">
        항상 허용:
    </text>
    <!-- Always allowed box -->
    <view_border
     bevel_style="none"
     follows="top|left"
     height="370"
     layout="topleft"
     left="10"
     top_pad="-5"
     width="453" />
    <name_list
     follows="left|top"
     height="370"
     layout="topleft"
     left_delta="0"
     multi_select="true"
     draw_heading="true"
     name="allowed_avatar_name_list"
     top_delta="0"
     width="453">
        <columns
         label="이름"
         name="name"
         width="453" />
    </name_list>
    <!-- Always allowed buttons -->
    <button
     follows="left|top"
     height="23"
     label="추가..."
     layout="topleft"
     left="10"
     name="add_allowed_avatar_btn"
     top_pad="6"
     width="114" />
    <button
     follows="left|top"
     height="23"
     label="제거..."
     layout="topleft"
     name="remove_allowed_avatar_btn"
     left_pad="6"
     width="114" />
  </panel>

  <!-- ============================= ALLOWED GROUPS tab ======================= -->
  <panel
    background_opaque="true"
    background_visible="true"
    bg_alpha_color="DkGray"
    bg_opaque_color="DkGray"
    bottom="-1"
    follows="all"
    label="허용 그룹"
    layout="topleft"
    left="0"
    help_topic="allowed_groups_tab"
    name="allowed_groups_panel"
    right="-1"
    top="0">
       <panel
        follows="left|top|right"
        height="27"
        label="상부_패널"
        layout="topleft"
        left="0"
        name="allowed_group_search_panel"
        right="-1"
        top="0">
           <filter_editor
            follows="left|top|right"
            height="23"
            layout="topleft"
            left="6"
            label="허용 그룹 검색"
            max_length_chars="300"
            name="allowed_group_search_input"
            top="4"
            width="395" />
           <button
            follows="left|top"
            height="23"
            label="복사"
            layout="topleft"
            left_pad="3"
            name="copy_allowed_group_list_btn"
            width="58" />
        </panel>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="20"
     layout="topleft"
     left="10"
     name="allow_group_label"
     top_pad="10"
     width="200">
        그룹을 항상 허용했다:
    </text>
    <!-- Groups always allowed box -->
    <view_border
     bevel_style="none"
     follows="top|left"
     height="370"
     layout="topleft"
     left="10"
     top_pad="-5"
     width="453" />
    <name_list
     follows="left|top"
     height="370"
     layout="topleft"
     left_delta="0"
     multi_select="true"
     draw_heading="true"
     name="allowed_group_name_list"
     top_delta="0"
     width="453">
        <columns
         label="이름"
         name="name"
         width="453" />
    </name_list>
    <!-- Groups always allowed buttons -->
    <button
     follows="left|top"
     height="23"
     label="추가..."
     layout="topleft"
     left="10"
     name="add_allowed_group_btn"
     top_pad="6"
     width="114" />
    <button
     follows="left|top"
     height="23"
     label="제거..."
     layout="topleft"
     name="remove_allowed_group_btn"
     left_pad="6"
     top_delta="0"
     width="114" />
  </panel>

  <!-- ================================ BANNED tab ============================ -->
  <panel
    background_opaque="true"
    background_visible="true"
    bg_alpha_color="DkGray"
    bg_opaque_color="DkGray"
    bottom="-1"
    follows="all"
    label="금지"
    layout="topleft"
    left="0"
    help_topic="banned_tab"
    name="banned_panel"
    right="-1"
    top="0">
       <panel
        follows="left|top|right"
        height="27"
        label="상부_패널"
        layout="topleft"
        left="0"
        name="banned_search_panel"
        right="-1"
        top="0">
           <filter_editor
            follows="left|top|right"
            height="23"
            layout="topleft"
            left="6"
            label="금지된 에이전트 검색"
            max_length_chars="300"
            name="banned_search_input"
            top="4"
            width="395" />
               <button
                follows="left|top"
                height="23"
                label="복사"
                layout="topleft"
                left_pad="3"
                name="copy_banned_list_btn"
                width="58" />
        </panel>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="20"
     layout="topleft"
     left="10"
     name="ban_resident_label"
     top_pad="10"
     width="200">
        항상 금지:
    </text>
    <!-- Always banned box -->
    <view_border
     bevel_style="none"
     follows="top|left"
     height="370"
     layout="topleft"
     left="10"
     top_pad="-5"
     width="453" />
      <name_list
       follows="left|top"
       height="370"
       layout="topleft"
       left_delta="0"
       multi_select="true"
       draw_heading="true"
       name="banned_avatar_name_list"
       top_delta="0"
       width="453">
          <columns
            label="이름"
            name="name"
            width="120" />
          <columns
            label="마지막 로그인 날짜"
            name="last_login_date"
            width="100" />
          <columns
            label="금지된 날짜"
            name="ban_date"
            width="100" />
          <columns
            label="금지 대상"
            name="bannedby"
            width="115" />
      </name_list>
    <!-- Always banned buttons -->
    <button
     follows="left|top"
     height="23"
     label="추가..."
     layout="topleft"
     left="10"
     name="add_banned_avatar_btn"
     top_pad="6"
     width="114" />
    <button
     follows="left|top"
     height="23"
     label="제거..."
     layout="topleft"
     name="remove_banned_avatar_btn"
     left_pad="6"
     top_delta="0"
     width="114" />
  </panel>


</tab_container>
</panel>

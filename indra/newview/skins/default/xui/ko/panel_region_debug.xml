<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="top|left"
 height="320"
 help_topic="panel_region_debug_tab"
 label="디버그"
 layout="topleft"
 left="0"
 name="Debug"
 top="320"
 width="480">
    <text
     follows="left|top"
     font="SansSerif"
     height="20"
     layout="topleft"
     left="10"
     name="region_text_lbl"
     top="10"
     width="100">
        지역:
    </text>
    <text
     follows="left|top"
     font="SansSerif"
     height="20"
     layout="topleft"
     left_delta="50"
     name="region_text"
     top_delta="0"
     width="400">
        알 수 없음
    </text>
    <check_box
     height="20"
     label="스크립트 사용 안 함"
     layout="topleft"
     left="10"
     name="disable_scripts_check"
     tool_tip="이 영역의 모든 스크립트 사용 안 함"
     top="30"
     width="80" />
    <check_box
     height="20"
     label="충돌 사용 안 함"
     layout="topleft"
     left="10"
     name="disable_collisions_check"
     tool_tip="이 영역에서 비아바타 충돌 사용 안 함"
     top="50"
     width="80" />
    <check_box
     height="20"
     label="물리학 사용 안 함"
     layout="topleft"
     left="10"
     name="disable_physics_check"
     tool_tip="이 영역의 모든 물리 사용 안 함"
     top="70"
     width="80" />
    <button
     enabled="false"
     follows="left|top"
     height="20"
     label="적용"
     layout="topleft"
     left="20"
     name="apply_btn"
     top="100"
     width="100" />

    <text
     follows="left|top"
     height="20"
     layout="topleft"
     left="10"
     name="objret_text_lbl"
     top="140"
     width="90">
        개체 반환
    </text>
    <text

     follows="left|top"
     height="20"
     layout="topleft"
     left="30"
     name="resident_text_lbl"
     top_pad="5"
     width="60">
        거주자:
    </text>
    <line_editor

     border_style="line"
     border_thickness="1"
     enabled="false"
     follows="left|top"
     height="20"
     layout="topleft"
     left_delta="60"
     mouse_opaque="false"
     name="target_avatar_name"
     top_delta="-2"
     width="270">
        (없음)
    </line_editor>
    <button
     follows="left|top"
     height="20"
     label="선택"
     layout="topleft"
     left_pad="5"
     name="choose_avatar_btn"
     top_delta="0"
     width="70" />
    <text
     follows="left|top"
     height="20"
     layout="topleft"
     left="30"
     name="options_text_lbl"
     top_pad="5"
     width="60">
        옵션:
    </text>
     <check_box
     height="20"
     label="스크립트 포함"
     layout="topleft"
     left_delta="60"
     name="return_scripts"
     tool_tip="스크립트가 있는 개체만 반환"
     top_delta="0"
     width="80" />
    <check_box
     height="20"
     label="다른 사람의 땅에서"
     layout="topleft"
     left_delta="0"
     name="return_other_land"
     tool_tip="다른 사람이 소유한 육지에 있는 물건만 반환"
     top_delta="20"
     width="80" />
    <check_box
     height="20"
     label="이 사유지의 모든 지역에서"
     layout="topleft"
     left_delta="0"
     name="return_estate_wide"
     tool_tip="이 자산을 구성하는 모든 지역의 개체 반환"
     top_delta="20"
     width="80" />
    <button
     follows="left|top"
     height="20"
     label="반환"
     layout="topleft"
     left="20"
     name="return_btn"
     top_pad="5"
     width="80" />
    <button
     follows="left|top"
     height="20"
     label="상단 충돌기 가져오기..."
     layout="topleft"
     left="10"
     name="top_colliders_btn"
     tool_tip="충돌이 발생할 가능성이 가장 높은 개체 목록"
     top_pad="20"
     width="130" />
    <button
     follows="left|top"
     height="20"
     label="최상위 스크립트 가져오기..."
     layout="topleft"
     left="10"
     name="top_scripts_btn"
     tool_tip="스크립트 실행에 가장 많은 시간을 소비하는 개체 목록"
     top_pad="5"
     width="130" />
    <button
     follows="left|top"
     height="20"
     label="지역 재시작"
     layout="topleft"
     left="10"
     name="restart_btn"
     tool_tip="선택한 시간(초)을 카운트다운하고 영역을 다시 시작합니다"
     top_pad="5"
     width="130" />
	<text follows="left|top" top_delta="5" left_delta="135" width="50" name="label_restart_delay">
		초:
	</text>
	<spinner
	 bottom_delta="0"
	 decimal_digits="0"
	 follows="left|top"
	 height="16"
	 increment="1"
	 initial_value="120"
	 left_delta="55"
	 max_val="3600"
	 min_val="15"
	 mouse_opaque="true"
	 name="restart_delay"
	 width="50"/>	 
    <button
     follows="left|top"
     height="20"
     label="재시작 취소"
     layout="topleft"
     left="10"
     name="cancel_restart_btn"
     tool_tip="Cancel region restart"
     top_pad="10"
     width="130" />
    <button
     follows="left|top"
     height="20"
     label="지역 디버그 콘솔"
     layout="topleft"
     left="10"
     name="region_debug_console_btn"
     tool_tip="지역 디버그 콘솔 열기"
     top_pad="5"
     width="150" />
</panel>

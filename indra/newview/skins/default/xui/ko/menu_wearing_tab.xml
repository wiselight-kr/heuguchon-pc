<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="Wearing">
    <menu_item_call
     label="터치"
     layout="topleft"
     name="touch_attach">
        <on_click
         function="Wearing.TouchAttach" />
    </menu_item_call>
    <menu_item_call
     label="편집"
     layout="topleft"
     name="edit_item">
        <on_click
         function="Wearing.EditItem" />
    </menu_item_call>
    <menu_item_call
     label="벗기"
     layout="topleft"
     name="take_off">
        <on_click
         function="Wearing.TakeOff" />
    </menu_item_call>
    <menu_item_call
     label="탈착"
     layout="topleft"
     name="detach">
        <on_click
         function="Wearing.Detach"
         parameter="detach"/>
    </menu_item_call>
    <menu_item_call
     label="벗기 / 탈착"
     layout="topleft"
     name="take_off_or_detach">
        <on_click
         function="Wearing.TakeOffDetach" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft"
     name="edit_outfit_separator" />
    <menu_item_call
     label="원본 표시"
     layout="topleft"
     name="show_original">
        <on_click
         function="Wearing.ShowOriginal" />
    </menu_item_call>
    <menu_item_call
     label="의상 편집"
     layout="topleft"
     name="edit_outfit">
        <on_click
         function="Wearing.EditOutfit" />
    </menu_item_call>
</context_menu>

<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 layout="topleft"
 left="1"
 top="1"
 width="540"
 name="general_panel"
 label="General">
 <!--language-->
    <text
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left="30"
     name="language_textbox"
     top="10"
     width="200">
        언어:
    </text>
    <combo_box
     control_name="Language"
     follows="left|top"
     height="23"
     layout="topleft"
     left="30"
     max_chars="135"
     name="language_combobox"
     width="150">
        <combo_box.item
         enabled="true"
         label="영어"
         name="English"
         value="en" />
        <!--
        <combo_box.item
         enabled="true"
         label="Dansk (Danish) - Beta"
         name="Danish"
         value="da" />
        -->
        <combo_box.item
         enabled="true"
         label="Deutsch (독일어)"
         name="Deutsch(German)"
         value="de" />
        <combo_box.item
         enabled="true"
         label="Español (스페인어) - Beta"
         name="Spanish"
         value="es" />
        <combo_box.item
         enabled="true"
         label="Français (불어) - Beta"
         name="French"
         value="fr" />
        <combo_box.item
         enabled="true"
         label="Italiano (이탈리아어) - Beta"
         name="Italian"
         value="it" />        
        <combo_box.item
         enabled="true"
         label="Polski (폴란드어)"
         name="Polish"
         value="pl" />
        <!--
        <combo_box.item
         enabled="true"
         label="Português (Portuguese) - Beta"
         name="Portugese"
         value="pt" />
        -->
        <combo_box.item
         enabled="true"
         label="Русский (러시아어) - Beta"
         name="Russian"
         value="ru" />
        <!--
        <combo_box.item
         enabled="true"
         label="Türkçe (Turkish) - Beta"
         name="Turkish"
         value="tr" />
        -->
        <combo_box.item
         enabled="true"
         label="한국어 (한국어) - Beta"
         name="Korean"
         value="ko" />
        <!--
        <combo_box.item
         enabled="true"
         label="Türkçe (Turkish) - Beta"
         name="Turkish"
         value="tr" />
        -->
        <combo_box.item
         enabled="true"
         label="日本語 (일본어) - Beta"
         name="(Japanese)"
         value="ja" />
        <!--
        <combo_box.item
         enabled="true"
         label="正體中文 (Traditional Chinese) - Beta"
         name="Traditional Chinese"
         value="zh" />
        -->
    </combo_box>
    <text
	 top_delta="4"
     font="SansSerifSmall"
     type="string"
     text_color="White_50"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left_pad="5"
     name="language_textbox2"
     width="130">
        (재시작이 필요함)
    </text>
<!--maturity-->
     <text
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left="30"
     top_pad="10"
     name="maturity_desired_prompt"
     width="200">
        등급이 지정된 콘텐츠에 액세스합니다:
    </text>
    <combo_box
     control_name="PreferredMaturity"
     follows="left|top"
     height="23"
     layout="topleft"
     left="30"
     name="maturity_desired_combobox"
     width="200">
        <combo_box.item
         label="일반, 적당한, 성인"
         name="Desired_Adult"
         value="42" />
        <combo_box.item
         label="일반적이고 적당한"
         name="Desired_Mature"
         value="21" />
        <combo_box.item
         label="일반"
         name="Desired_PG"
         value="13" />
        <combo_box.commit_callback
         function="Pref.MaturitySettings"/>
    </combo_box>
    <!-- this text will never show at the same time as the above combo, they can overlap -->
    <text
     top_delta="4"
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     name="maturity_desired_textbox"
     width="200">
    </text>
    <icon
     follows="left|top"
     height="16"
     image_name="Parcel_PG_Dark"
     layout="topleft"
     left_pad="5"
     name="rating_icon_general"
     top_delta="-1"
     width="18"/>
    <icon
     follows="left|top"
     height="16"
     image_name="Parcel_M_Dark"
     layout="topleft"
     left_pad="5"
     name="rating_icon_moderate"
     width="18"/>
    <icon
     follows="left|top"
     height="16"
     image_name="Parcel_R_Dark"
     layout="topleft"
     left_pad="5"
     name="rating_icon_adult"
     width="18"/>
<!--start location-->
    <text
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left="30"
     name="start_location_textbox"
     top_pad="8"
     width="200">
        시작 위치:
    </text>
    <combo_box
     control_name="LoginLocation"
     follows="left|top"
     height="23"
     layout="topleft"
     name="start_location_combo"
     left="30"
     width="150">
        <combo_box.item
         label="마지막 위치"
         name="MyLastLocation"
         value="last" />
        <combo_box.item
         label="내 잡"
         name="MyHome"
         value="home" />
    </combo_box>
    <check_box
     control_name="ShowStartLocation"
     height="16"
     initial_value="true"
     label="로그인에서 보기"
     layout="topleft"
     left_pad="5"
     name="show_location_checkbox"
     top_delta="5"
     width="256"
	 tool_tip="" />
<!--name tags-->
    <text
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left="30"
     name="name_tags_textbox"
     top_pad="15"
     width="200">
       이름 태그:
    </text>
    <radio_group
     control_name="AvatarNameTagMode"
     height="20"
     width="200"
     layout="topleft"
     left="30"
     top_pad="0"
     name="Name_Tag_Preference">
        <radio_item
         label="끄기"
         name="radio"
         top_delta="20"
         layout="topleft"
         height="16"
         left="0"
         value="0"
         width="50" />
        <radio_item
         label="켜기"
         left_pad="0"
         layout="topleft"
         top_delta="0"
         height="16"
         name="radio2"
         value="1"
         width="50" />
        <radio_item
         label="간단히 보기"
         left_pad="0"
         name="radio3"
         height="16"
         layout="topleft"
         top_delta="0"
         value="2"
         width="75" />
    </radio_group>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left_pad="0"
     name="RenderNameShowTime_LabelPrefix"
     top_delta="0"
     width="5">
       (
    </text>
    <spinner
     allow_text_entry="true"
     control_name="RenderNameShowTime"
     decimal_digits="0"
     follows="left|top"
     height="20"
     increment="1"
     layout="topleft"
     left_pad="0"
     max_val="60"
     min_val="1"
     name="RenderNameShowTime"
     top_delta="-4"
     width="40"/>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="15"
     layout="topleft"
     left_pad="3"
     name="RenderNameShowTime_LabelSuffix"
     top_delta="4"
     width="80">
       초)
    </text>
    <check_box
     enabled_control="AvatarNameTagMode"
     control_name="RenderNameShowSelf"
     height="16"
     label="내 이름"
     layout="topleft"
     left="30"
     name="show_my_name_checkbox1"
     top_pad="8"
     width="140"
	 tool_tip="" />
    <check_box
     control_name="NameTagShowUsernames"
     height="16"
     label="사용자 이름"
     layout="topleft"
     left_pad="50"
     name="show_slids"
     tool_tip="사용자 이름 표시, bobsmith123같은"
     width="100"
     top_delta="0" />

    <check_box
     control_name="NameTagShowGroupTitles"
     enabled_control="AvatarNameTagMode"
     height="16"
     label="그룸 제목"
     layout="topleft"
     left="30"
     width="140"
     name="show_all_title_checkbox1"
     tool_tip="임원 또는 구성원과 같은 그룹 제목 표시"
     top_pad="3" />

    <check_box
     control_name="RenderHideGroupTitle"
     height="16"
     label="자신의 그룹 제목 숨기기"
     layout="topleft"
     left_pad="50"
     name="RenderHideGroupTitle"
     tool_tip="자신과 다른 모든 사용자의 그룹 제목을 표시하거나 숨깁니다."/>

    <check_box
     control_name="UseDisplayNames"
     follows="top|left"
     height="16"
     label="표시 이름 보기"
     layout="topleft"
     left="30"
     name="display_names_check"
     width="140"
     tool_tip="대화, IM, 이름 태그 등에 표시 이름을 사용하려면 이 항목을 선택합니다.."
     top_pad="3"/>
   <check_box
     control_name="FSNameTagShowLegacyUsernames"
     height="16"
     label="사용자 이름 대신 레거시 이름"
     layout="topleft"
     left_pad="50"
     name="FSshow_legacyun"
     tool_tip="사용자 이름(firstname.lastname) 대신 레거시 이름(Firstname Lastname) 표시"
     top_delta="0" />
    
   <check_box
     control_name="NameTagShowFriends"
     enabled_control="AvatarNameTagMode"
     height="16"
     label="친구들 하이라이트"
     layout="topleft"
     left="30"
     width="140"
     top_pad="3"
     name="show_friends"
     tool_tip="친구의 이름 태그를 강조 표시."
     top_delta="0" />

  <check_box
   enabled_control="FSNameTagShowLegacyUsernames"
   control_name="FSTrimLegacyNames"
   height="16"
   label="레거시 이름에서 'Resident' 자르기"
   layout="topleft"
   left_pad="50"
   name="legacy_trim_check"
   width="100"
   tool_tip="레거시 이름에서 'Resident' 자르기"
   top_delta="0"/>
    
   <check_box
     control_name="FSLegacyNametagPosition"
     enabled_control="AvatarNameTagMode"
     height="16"
     label="레거시 이름 태그 위치 지정"
     layout="topleft"
     left="30"
     width="140"
     top_pad="3"
     name="FSLegacyNametagPosition"
     tool_tip="활성화된 경우 이름 태그는 아바타 위치에 고정되어 있으며 애니메이션으로 인해 움직이는 경우 아바타 헤드를 따르지 않습니다."
     top_delta="0" />

  <check_box
   enabled_control="AvatarNameTagMode"
   control_name="FSShowAutorespondInNametag"
   height="16"
   label="이름 태그에 자동응답 보기"
   layout="topleft"
   left_pad="50"
   name="FSShowAutorespondInNameTag"
   width="100"
   tool_tip="사용자 이름 태그에 자동 응답이 활성화되어 있는지 여부를 표시합니다."
   top_delta="0"/>
    
   <check_box
     control_name="FSTagShowARW"
     enabled_control="AvatarNameTagMode"
     height="16"
     label="아바타 복잡성 표시"
     layout="topleft"
     left="30"
     width="140"
     top_pad="3"
     name="FSTagShowARW"
     tool_tip="활성화된 경우 이름 태그에 현재 아바타 복잡성이 표시됩니다."/>
   <check_box
     control_name="FSTagShowTooComplexOnlyARW"
     height="16"
     label="너무 복잡한 경우에만"
     layout="topleft"
     left="40"
     width="140"
     top_pad="3"
     name="FSTagShowTooComplexOnlyARW"
     tool_tip="사용 가능한 경우, 이름 태그는 아바타가 너무 복잡하고 젤리 돌로 표시되는 경우에만 현재 아바타 복잡도를 표시합니다."/>
   <check_box
     control_name="FSTagShowOwnARW"
     height="16"
     label="복잡성 표시"
     layout="topleft"
     left="40"
     width="140"
     top_pad="3"
     name="FSTagShowOwnARW"
     tool_tip="활성화된 경우 사용자 이름 태그는 현재 아바타 복잡도를 나타냅니다."/>

<!--away/logout-->
	 <text
     type="string"
     length="1"
     follows="left|top"
     height="13"
     layout="topleft"
     left="30"
     top_pad="10"
     name="title_afk_text"
     width="260">
        어웨이 시간초과:
    </text>

    <text
     type="string"
     length="1"
     follows="left|top"
     height="13"
     layout="topleft"
     left_pad="0"
     top_delta="0"
     name="title_afk_quit_text"
     width="200">
        로그아웃 표시 후 로그아웃:
    </text>

    <combo_box
     height="23"
     layout="topleft"
     control_name="AFKTimeout"
     left="30"
     top_pad="5"
	 label="어웨이 시간초과:"
     top_delta="0"
     name="afk"
     width="100"
	 tool_tip="">
        <combo_box.item
         label="2 분"
         name="item0"
         value="120" />
        <combo_box.item
         label="5 분"
         name="item1"
         value="300" />
        <combo_box.item
         label="10 분"
         name="item2"
         value="600" />
        <combo_box.item
         label="30 분"
         name="item3"
         value="1800" />
        <combo_box.item
         label="60 분"
         name="item4"
         value="3600" />
        <combo_box.item
         label="안됨"
         name="item5"
         value="0" />
    </combo_box>

    <check_box
     control_name="AvatarSitOnAway"
     name="AvatarSitOnAway"
     follows="top|left"
     height="16"
     label="어웨이 일 떄 앉기"
     layout="topleft"
     left_pad="5"
     top_delta="4"
     width="150"
	 tool_tip=""/>

    <combo_box
     height="23"
     layout="topleft"
     control_name="QuitAfterSecondsOfAFK"
     left_pad="5"
     top_delta="-4"
     name="quitafk"
     width="100"
	 tool_tip="">
        <combo_box.item
         label="2 분"
         name="item0"
         value="120" />
        <combo_box.item
         label="5 분"
         name="item1"
         value="300" />
        <combo_box.item
         label="10 분"
         name="item2"
         value="600" />
        <combo_box.item
         label="30 분"
         name="item3"
         value="1800" />
		<combo_box.item
		 label="안됨"
		 name="item4"
		 value="0" />
    </combo_box>

    <text
     type="string"
     length="1"
     follows="left|top"
     height="13"
     layout="topleft"
     left="30"
     mouse_opaque="false"
     name="text_box3"
     top_pad="10"
     width="500">
       사용 불가능 및 자동 응답 메시지는 개인 정보 / 자동 응답에서 찾을 수 있습니다.
    </text>

</panel>

<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 height="205"
 layout="topleft"
 name="build options floater"
 help_topic="build_options_floater"
 save_rect="true"
 title="그리드 옵션"
 width="264">
    <spinner
     control_name="GridResolution"
     follows="left|top"
     height="23"
     initial_value="1"
     label="유닛 (미터)"
     label_width="172"
     layout="topleft"
     left="10"
     max_val="5"
     min_val="0.01"
     name="GridResolution"
     top_pad="30"
     width="242" />
    <spinner
     control_name="GridDrawSize"
     decimal_digits="1"
     follows="left|top"
     height="23"
     increment="0.5"
     initial_value="5"
     label="규모 (미터)"
     label_width="172"
     layout="topleft"
     left_delta="0"
     max_val="50"
     min_val="1"
     name="GridDrawSize"
     top_pad="0"
     width="242" />
    <spinner
     control_name="FSBuildToolDecimalPrecision"
     decimal_digits="0"
     follows="left|top"
     height="23"
     increment="1"
     initial_value="5"
     label="십진법 정확성"
     label_width="172"
     layout="topleft"
     left_delta="0"
     max_val="7"
     min_val="0"
     name="DecimalPrecision"
     top_pad="0"
     width="242" />
    <spinner
     control_name="RotationStep"
     decimal_digits="2"
     follows="left|top"
     height="23"
     increment="0.01"
     label="각도 (degrees)"
     label_width="172"
     layout="topleft"
     left_delta="0"
     max_val="359.99"
     min_val="0.01"
     name="RotationStep"
     top_pad="0"
     width="242"
	 tool_tip="회전 도구를 통한 모든 회전은 본 장치의 배수로 제한됩니다 [기본값: 1.0]"/>
    <check_box
     control_name="GridSubUnit"
     height="16"
     label="하위 장치에 스냅"
     layout="topleft"
     name="GridSubUnit"
     top_pad="0"
     width="200" />
    <check_box
     control_name="GridCrossSections"
     height="16"
     label="횡단면 보기"
     layout="topleft"
     name="GridCrossSection"
     top_pad="5"
     width="200" />
   <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left="10"
         tool_tip="Grid opacity"
         name="grid_opacity_label"
         top_pad="5"
         width="128">
            투명도:
   </text>
    <slider
     control_name="GridOpacity"
     follows="left"
     height="18"
     increment="0.05"
     initial_value="0.7"
     layout="topleft"
     name="GridOpacity"
     show_text="false"
     top_pad="0"
     width="210" />
</floater>

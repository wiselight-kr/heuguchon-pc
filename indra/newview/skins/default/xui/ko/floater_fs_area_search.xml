			 top_pad="10"
			 initial_value="true">
				<check_box.commit_callback
				 function="AreaSearch.DisplayColumn"
				 parameter="land_impact"/>
				<check_box.on_check
				 function="AreaSearch.EnableColumn"
				 parameter="land_impact"/>
			</check_box>
			<check_box
			 follows="top|left"
			 height="16"
			 layout="topleft"
			 left="8"
			 name="show_prim_count"
			 label="프림 세기"

			</check_box>
			<check_box
			 follows="top|left"
			 height="16"
			 layout="topleft"
			 left="8"
			 name="show_owner"
			 label="소유주"
			 top_pad="10"
			 initial_value="true">
				<check_box.commit_callback
				 function="AreaSearch.DisplayColumn"
				 parameter="owner"/>
				<check_box.on_check
				 function="AreaSearch.EnableColumn"
				 parameter="owner"/>
			</check_box>
			<check_box
			 follows="top|left"
			 height="16"
			 layout="topleft"
			 left="8"
			 name="show_group"
			 label="그룹"
			 top_pad="10"
			 initial_value="true">
				<check_box.commit_callback
				 function="AreaSearch.DisplayColumn"
				 parameter="group"/>
				<check_box.on_check
				 function="AreaSearch.EnableColumn"
				 parameter="group"/>
			</check_box>
			<check_box
			 follows="top|left"
			 height="16"
			 layout="topleft"
			 left="8"
			 name="show_creator"
			 label="창조주"
			 top_pad="10"
			 initial_value="true">
				<check_box.commit_callback
				 function="AreaSearch.DisplayColumn"
				 parameter="creator"/>
				<check_box.on_check
				 function="AreaSearch.EnableColumn"
				 parameter="creator"/>
			</check_box>
			<check_box
			 follows="top|left"
			 height="16"
			 layout="topleft"
			 left="8"
			 name="show_last_owner"
			 label="마지막 소유자"
			 top_pad="10"
			 initial_value="true">
				<check_box.commit_callback
				 function="AreaSearch.DisplayColumn"
				 parameter="last_owner"/>
				<check_box.on_check
				 function="AreaSearch.EnableColumn"
				 parameter="last_owner"/>

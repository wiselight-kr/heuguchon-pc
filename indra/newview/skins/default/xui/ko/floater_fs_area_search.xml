<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater
 name="area_search"
 help_topic="fs_area_search"
 title="개체 영역 검색"
 min_width="430"
 min_height="395"
 width="610"
 height="435"
 save_rect="true"
 single_instance="true"
 reuse_instance="false"
 can_resize="true"
 can_minimize="true"
 can_close="true"
 can_drag_on_left="false">
	<tab_container
	 follows="all"
	 layout="topleft"
	 left="1"
	 right="-1"
	 name="area_searchtab"
	 tab_position="top"
	 tab_height="20"
	 tab_min_width="67"
	 bottom="430"
	 top="2">
		<panel
		 border="true"
		 follows="all"
		 label="List"
		 layout="topleft"
		 left="0"
		 name="area_search_list_panel"
		 top="0"
		 height="435"
		 width="610">
			<panel.string name="ListedPendingTotalBlank">
				목록 | 보류 | 종합
			</panel.string>
			<panel.string name="ListedPendingTotalFilled">
				[LISTED] 목록 | [PENDING] 보류 | [TOTAL] 종합
			</panel.string>
			<fs_scroll_list
			 name="result_list"
			 left="5"
			 right="-5"
			 height="395"
			 top="5"
			 follows="left|top|bottom|right"
			 column_padding="0"
			 draw_heading="true"
			 multi_select="true"
			 search_column="1">
				<fs_scroll_list.columns
				 name="distance"
				 label="Dist"
				 width="40"
				 tool_tip="아바타에서 개체까지의 거리."/>
				<fs_scroll_list.columns
				 name="name"
				 label="이름"
				 width="180"/>
				<fs_scroll_list.columns
				 name="description"
				 label="묘사"
				 width="180"/>
				<fs_scroll_list.columns
				 name="price"
				 label="가격"
				 width="40"
				 tool_tip="만약 그 물건이 판매된다면, 당신이 그것을 사기 위해 지불할 금액."/>
				<fs_scroll_list.columns
				 name="land_impact"
				 label="랜드 충격"
				 width="40"
				 tool_tip="소포의 최대값과 비교하여 계산되는 숫자."/>
				<fs_scroll_list.columns
				 name="prim_count"
				 label="프리즘"
				 width="40"
				 tool_tip="각 객체를 구성하는 프림 수."/>
				<fs_scroll_list.columns
				 name="owner"
				 label="소유주"
				 width="140"/>
				<fs_scroll_list.columns
				 name="group"
				 label="그룹"
				 width="140"
				 tool_tip="개체가 설정된 그룹."/>
				<fs_scroll_list.columns
				 name="creator"
				 label="창조주"
				 width="170"/>
				<fs_scroll_list.columns
				 name="last_owner"
				 label="마지막 소유주"
				 width="150"
				 tool_tip="누가 마지막으로 소유했는지."/>
			</fs_scroll_list>
			<button
			 name="Refresh"
			 label="새로고침"
			 font="SansSerif"
			 mouse_opaque="true"
			 height="20"
			 width="80"
			 left="10"
			 bottom="-6"
			 follows="left|bottom"/>
			<text
			 name="counter"
			 height="15"
			 width="220"
			 left_delta="85"
			 bottom="-9"
			 follows="left|bottom">
				목록 | 보류 | 종합
			</text>
			<check_box
			 follows="right|bottom"
			 height="16"
			 left_pad="180"
			 width="80"
			 name="beacons"
			 label="Show beacons"
			 bottom="-9"/>
		</panel>
		<panel
		 border="true"
		 follows="all"
		 label="찾기"
		 layout="topleft"
		 left="0"
		 top="0"
		 help_topic="area_search_find_tab"
		 name="area_search_find_panel" >
			<text
			 name="name_label"
			 top_pad="10"
			 follows="top|left"
			 height="15"
			 left="12"
			 width="200">
				이름:
			</text>
			<text
			 name="desc_label"
			 bottom_delta="0"
			 follows="top|left"
			 height="15"
			 left="218"
			 width="200">
				묘사:
			</text>
			<line_editor
			 name="name_search"
			 bevel_style="in"
			 border_style="line"
			 border_thickness="1"
			 height="20"
			 width="200"
			 left="10"
			 bottom_delta="20"
			 follows="left|top"
			 font="SansSerifSmall"
			 max_length_chars="256"
			 mouse_opaque="true" />
			<line_editor
			 name="description_search"
			 bevel_style="in"
			 border_style="line"
			 border_thickness="1"
			 height="20"
			 width="200"
			 left_delta="206"
			 bottom_delta="0"
			 follows="left|top"
			 font="SansSerifSmall"
			 max_length_chars="256"
			 mouse_opaque="true" />
			<text
			 name="owner_label"
			 bottom_delta="20"
			 follows="top|left"
			 height="15"
			 left="12"
			 width="200">
				소유주:
			</text>
			<text
			 name="group_label"
			 bottom_delta="0"
			 follows="top|left"
			 height="15"
			 left="218"
			 width="200">
				그룹:
			</text>
			 <line_editor
			 name="owner_search"
			 bevel_style="in"
			 border_style="line"
			 border_thickness="1"
			 height="20"
			 width="200"
			 left="10"
			 bottom_delta="20"
			 follows="left|top"
			 font="SansSerifSmall"
			 max_length_chars="256"
			 mouse_opaque="true" />
			<line_editor
			 name="group_search"
			 bevel_style="in"
			 border_style="line"
			 border_thickness="1"
			 height="20"
			 width="200"
			 left_delta="206"
			 bottom_delta="0"
			 follows="left|top"
			 font="SansSerifSmall"
			 max_length_chars="256"
			 mouse_opaque="true"/>
			<text
			 name="creator_label"
			 bottom_delta="20"
			 follows="top|left"
			 height="15"
			 left="12"
			 width="200">
				창조주:
			</text>
			<text
			 name="last_owner_label"
			 bottom_delta="0"
			 follows="top|left"
			 height="15"
			 left="218"
			 width="200">
				마지막 소유주:
			</text>
			 <line_editor
			 name="creator_search"
			 bevel_style="in"
			 border_style="line"
			 border_thickness="1"
			 height="20"
			 width="200"
			 left="10"
			 bottom_delta="20"
			 follows="left|top"
			 font="SansSerifSmall"
			 max_length_chars="256"
			 mouse_opaque="true"/>
			 <line_editor
			 name="last_owner_search"
			 bevel_style="in"
			 border_style="line"
			 border_thickness="1"
			 height="20"
			 width="200"
			 left_delta="206"
			 bottom_delta="0"
			 follows="left|top"
			 font="SansSerifSmall"
			 max_length_chars="256"
			 mouse_opaque="true"/>
			<check_box
			 follows="top|left"
			 height="16"
			 layout="topleft"
			 left="8"
			 name="regular_expression"
			 label="정규 표현식 사용"
			 top_pad="10"/>
			<button
			 name="search"
			 label="검색"
			 mouse_opaque="true"
			 height="20"
			 width="80"
			 left="10"
			 bottom="-6"
			 follows="left|bottom"/>
			<button
			 name="clear"
			 label="지우기"
			 mouse_opaque="true"
			 height="20"
			 width="80"
			 left_pad="10"
			 bottom="-6"
			 follows="left|bottom"/>
		</panel>
		<panel
		 border="true"
		 follows="all"
		 label="필터"
		 layout="topleft"
		 left="0"
		 top="0"
		 help_topic="area_search_filter_tab"
		 name="area_search_filter_panel" >
			<text
			 name="only_list"
			 height="15"
			 follows="top|left"
			 right="-5"
			 left="5">

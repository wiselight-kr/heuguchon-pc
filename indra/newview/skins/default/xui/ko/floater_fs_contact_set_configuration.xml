<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 height="400"
 width="380"
 layout="topleft"
 can_resize="false"
 legacy_header_height="18"
 name="floater_fs_contact_set_configure"
 title="연락처 집합 설정">
    <floater.string
     name="title">
    [NAME] 설정
    </floater.string>
    <panel
     name="top_panel"
     top="18"
     left="0"
     height="120"
     width="380"
     border_visible="true"
     bevel_style="none">
    <text
     top="10"
     follows="left|top"
     layout="topleft"
     left="10"
     height="12"
     name="set_label">
연락처 집합 설정:
    </text>
    <line_editor
     name="set_name_editor"
     height="23"
     top_pad="10"
     follows="left|top"
     layout="topleft"
     left="14"
     right="-90"/>
    <button
     name="rename_btn"
     height="23"
     top_delta="0"
     follows="left|top|right"
     layout="topleft"
     right="-10"
     width="75"
     label="개명"/>
    <color_swatch
     can_apply_immediately="true"
     follows="left|top"
     height="55"
     label="색깔 선택"
     top_pad="8"
     left="14"
     name="set_swatch"
     tool_tip="색깔 선택기를 열려면 클릭하십시오."
     width="65" />
    <check_box
     height="16"
     label="Show notifications"
     layout="topleft"
     left_pad="4"
     initial_value="false"
     name="show_set_notifications"
     top_delta="0"
     width="400" />
    </panel>
    <panel
     name="bottom_panel"
     top_pad="0"
     height="260"
     width="380"
     border_visible="true"
     bevel_style="none">
    <text
     top="10"
     follows="left|top"
     layout="topleft"
     left="10"
     height="12"
     name="global_label">
전역 설정:
    </text>
    <color_swatch
     can_apply_immediately="true"
     follows="left|top"
     height="55"
     label="기본 설정 색깔"
     top_pad="8"
     left_delta="4"
     name="global_swatch"
     tool_tip="색깔 선택기를 열려면 클릭하십시오."
     width="100" />
    <text
     top_pad="-2"
     follows="left|top"
     height="16"
     left="15"
     name="label_online"
     width="200">
온라인 알림:
    </text>
    <check_box
     top_pad="4"
     follows="left|top"
     height="12"
     label="모든 친구를 위한 온라인 공지사항을 건배로 표시"
     left_delta="5"
     name="ChatOnlineNotification"
     width="270"
     control_name="ChatOnlineNotification"/>
    <check_box
     follows="left|top"
     height="12"
     label="근처 채팅에 있는 모든 친구의 온라인 공지사항 표시"
     name="OnlineOfflinetoNearbyChat"
     width="270"
     control_name="OnlineOfflinetoNearbyChat"/>
    <check_box
     disabled_control="ChatOnlineNotification"
     follows="left|top"
     height="12"
     label="특정 세트에 대한 온라인 공지사항을 토스트로 표시"
     name="FSContactSetsNotificationToast"
     width="270"
     control_name="FSContactSetsNotificationToast"/>
    <check_box
     disabled_control="OnlineOffLinetoNearbyChat"
     follows="left|top"
     height="12"
     label="특정 세트에 대한 온라인 공지사항을 근처 채팅에 표시"
     name="FSContactSetsNotificationNearbyChat"
     width="270"
     control_name="FSContactSetsNotificationNearbyChat"/>
    <text
     top_pad="8"
     follows="left|top"
     height="16"
     left="15"
     name="label_colors"
     width="200">
사용자 설정 색깔:
    </text>
    <check_box
     top_pad="4"
     follows="left|top"
     height="12"
     label="연락처 집합을 기준으로 한 색상 미니 맵 아이콘"
     left_delta="5"
     name="FSContactSetsColorizeMiniMap"
     width="270"
     control_name="FSContactSetsColorizeMiniMap"/>
    <check_box
     follows="left|top"
     height="12"
     label="연락처 집합에 기반한 색상 이름 태그"
     name="FSContactSetsColorizeNameTag"
     width="270"
     control_name="FSContactSetsColorizeNameTag"/>
    <check_box
     follows="left|top"
     height="12"
     label="연락처 세트에 기반한 컬러 레이더 목록 항목"
     name="FSContactSetsColorizeRadar"
     width="270"
     control_name="FSContactSetsColorizeRadar"/>
    <check_box
     follows="left|top"
     height="12"
     initial_value="false"
     label="연락처 집합에 기반한 색깔 채팅"
     name="FSContactSetsColorizeChat"
     width="270"
     control_name="FSContactSetsColorizeChat"/>
    </panel>
</floater>

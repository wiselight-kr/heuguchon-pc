<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 can_minimize="false"
 height="495"
 layout="topleft"
 name="Image Preview"
 help_topic="image_preview"
 width="300">
    <text
     type="string"
     length="1"
     bottom_delta="35"
     follows="top|left"
     height="15"
     layout="topleft"
     left="10"
     name="name_label">
        이름:
    </text>
    <line_editor
     follows="top|left|right"
     height="19"
     layout="topleft"
     left_delta="0"
     name="name_form"
     top_pad="0"
     width="280" />
    <text
     type="string"
     length="1"
     bottom_delta="20"
     follows="top|left"
     height="15"
     layout="topleft"
     name="description_label">
        묘사:
    </text>
    <line_editor
     follows="top|left|right"
     height="19"
     layout="topleft"
     left_delta="0"
     name="description_form"
     top_pad="0"
     width="280" />
    <text
     type="string"
     length="1"
     bottom_delta="30"
     follows="top|left"
     height="25"
     width="105"
     word_wrap="true"
     layout="topleft"
     name="preview_label">
        이미지 미리보기:
    </text>
    <combo_box
     height="18"
     label="의상 종류"
     layout="topleft"
     left_delta="110"
     name="clothing_type_combo"
     top_delta="-2"
     width="160">
        <item
         label="이미지"
         value="이미지"
         name="Image" />
        <item
         label="머리카락"
         value="머리카락"
         name="Hair" />
        <item
         label="여성 머리"
         value="여성 머리"
         name="FemaleHead" />
        <item
         label="여성 상체"
         value="여성 상체"
         name="FemaleUpperBody" />
        <item
         label="여성 하체"
         value="여성 하체"
         name="FemaleLowerBody" />
        <item
         label="남성 머리"
         value="남성 머리"
         name="MaleHead" />
        <item
         label="남성 상체"
         value="남성 상체"
         name="MaleUpperBody" />
        <item
         label="남성 하체"
         value="남성 하체"
         name="MaleLowerBody" />
        <item
         label="치마"
         value="치마"
         name="Skirt" />
        <item
         label="조각된 프림"
         value="조각된 프림"
         name="SculptedPrim" />
    </combo_box>
    <text
     type="string"
     length="1"
     bottom="250"
     height="45"
     word_wrap="true"
     follows="top|left"
     layout="topleft"
     left="10"
     name="bad_image_text">
        이미지를 읽을 수 없습니다.

이미지를 24비트 Targa로 저장해 보십시오. (.tga).
    </text>
    <check_box
     enabled="false"
     initial_value="false"
     follows="bottom|left"
     height="16"
     label="무손실 압축 사용"
     layout="topleft"
     left_delta="2"
     name="lossless_check"
     top_pad="162"
     width="280" />
     <check_box
      control_name="TemporaryUpload"
      visible="false"
      follows="bottom|left"
      height="16"
      initial_value="false"
      left_delta="155"
      name="temp_check"
      width="280"
      label="임시 (무료)"
       tool_tip="자산을 임시 자산으로 설정합니다. 즉, 무상 자산이지만, 자산이 더 이상 존재하지 않을 때까지 짧은 시간 동안만 유효합니다." />
    <button
     follows="bottom|right"
     height="20"
     label="취소"
     layout="topleft"
     left="165"
     name="cancel_btn"
     top="430"
     width="125" />
    <button
     follows="bottom|left"
     height="20"
     label="업로드 (L$[AMOUNT])"
     layout="topleft"
     left_delta="-150"
     name="ok_btn"
     top_delta="0"
     width="125" />
    <text
     type="string"
     length="1"
     follows="left|top"
     height="35"
     layout="topleft"
     left="10"
     mouse_opaque="false"
     skip_link_underline="true"
     name="info_text"
     word_wrap="true"
     top_pad="10"
     width="270"/>
</floater>

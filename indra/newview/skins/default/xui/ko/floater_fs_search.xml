		name="people_friend_btn"
		width="120"
		left_pad="1" />
		<button
		layout="topleft"
		follows="left|bottom"
		height="23"
		label="프로필 열기"
		name="group_profile_btn"
		top="484"
		left="3"
		width="120" />
		<button
		layout="topleft"
		follows="left|bottom"
		height="23"
		label="채팅 참가"
		name="group_message_btn"
		width="120"
		left_pad="1" />
		<button
		layout="topleft"
		follows="left|bottom"
		height="23"
		label="그룹 참가"
		name="group_join_btn"
		width="120"
		left_pad="1" />

		layout="topleft"
		left="20"
		top_pad="2"
		name="maturity_icon"
		width="18" />
		<text_editor
		left_pad="4"
		top_delta="0"
		height="28"
		width="302"
		layout="topleft"
		follows="left|top|right"
		name="location"
		bg_visible="false"
		border_visible="false"
		h_pad="0"
		v_pad="0"
		word_wrap="true"
		enabled="false"
		max_length="117"
		allow_scroll="false"

         layout="topleft"
         left="15"
         min_val="0"
         max_val="120"
         name="PitchFromMousePositionSlider"
         top_pad="10"
         width="471" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left="485"
         name="PitchFromMousePositionText"
         top_delta="0"
         width="50">
            각도
        </text>
        <slider
         control_name="YawFromMousePosition"
         decimal_digits="0"
         follows="left|top"
         height="16"
         increment="5"
         label="아바타의 머리가 마우스를 따라가는 수평 범위:"
         tool_tip="아바타의 머리가 마우스(요)를 따르는 수평 범위(도)"
         label_width="300"
         layout="topleft"
         left="15"
         min_val="0"
         max_val="120"
         name="YawFromMousePositionSlider"
         top_pad="5"
         width="471" />
        <text
         type="string"
         length="1"
         follows="left|top"
         height="16"
         layout="topleft"
         left="485"
         name="YawFromMousePositionText"
         top_delta="0"
         width="50">
            각도
        </text>
        <text
         top_pad="5"
         left="15"
         name="note_lookat"
         follows="left|top"
         layout="topleft"
         text_color="White_50"
         height="12"
         width="500">
             (아바타 조회 대상 설정은 개인 정보 탭에 있습니다.)

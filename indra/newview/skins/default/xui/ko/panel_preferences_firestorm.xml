<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 layout="topleft"
 left="1"
 top="1"
 width="540"
 label="Firestorm"
 name="firestorm">
 
<panel.string name="BeamsOffLabel">
	=== OFF ===
</panel.string>
	<string name="EmbeddedItemNotSet">
		설정 안 됨
	</string>
	<string name="EmbeddedItemNotAvailable">
		이용 불가능
	</string>
	<string name="EmbeddedItemNotLoggedIn">
		로그인하지 않음
	</string>

<tab_container
 label="Firestorm 환경설정"
 layout="topleft"
 follows="all"
 top="5"
 bottom="-1"
 left="1"
 right="-1"
 name="tabs"
 tab_min_width="50"
 tab_position="top" >

    <!--Extras-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="엑스트라"
     name="tab-extras" >
        <check_box
         control_name="RestrainedLove"
         layout="topleft"
         follows="left|top"
         top_pad="6"
         left="10"
         width="270"
         height="20"
         name="checkMiscRLVa"
         label="스크립트로 작성된 원격 뷰어 컨트롤 허용  (RLVa)"/>
        <text
         layout="topleft"
         follows="left|top"
         left_pad="5"
         height="16"
         width="150"
         name="textMiscRLVaRestart"
         text_color="White_25">
            (재시작이 요구됨)
        </text>
        <check_box
         top_pad="3"
         left="10"
         follows="left|top"
         height="16"
         label="가능한 경우 항상 랜드 그룹 아래의 객체 크기 조정"
         name="grouplandrez"
         width="270"
         control_name="RezUnderLandGroup"
         tool_tip="현재 그룹 태그에 관계 없이 랜드 그룹에 속해 있는 경우 랜드 그룹 아래의 객체 크기를 조정하려고 시도."/>
		<check_box
		 control_name="EffectScriptChatParticles"
		 height="16"
		 label="스크립트가 통신할 때 입자 효과 생성"
		 layout="topleft"
		 name="EffectScriptChatParticles"
		 tool_tip="사용하도록 설정된 경우 스크립트가 통신할 때 스크립트 개체가 소용돌이치는 입자 조명을 표시합니다."
		 initial_value="true"
		 top_pad="3"
		 width="350" />
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         label="대상 아바타에 도달할 때 추적 비콘 비활성화 (&lt;= 3m)"
         name="FSDisableAvatarTrackerAtCloseIn"
         width="270"
         control_name="FSDisableAvatarTrackerAtCloseIn"
         tool_tip="활성화된 경우 표적 아바타까지의 거리가 3m 미만인 경우 추적 비콘이 자동으로 비활성화됩니다. (기본값)"/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         label="도시락 유휴 애니메이션 수정"
         name="play_default_bento_idle_animation_toggle"
         tool_tip="활성화된 경우, 뷰어는 다른 도시락 애니메이션이 실행되지 않을 때 손, 날개, 입 및 꼬리를 자연스러운 위치로 포즈하는 기본 우선 순위 0 도시락 애니메이션을 실행합니다."
         width="270"
         control_name="FSPlayDefaultBentoAnimation"/>
        <check_box
         top_pad="10"
         follows="left|top"
         height="16"
         label="로그인 진행률 화면 비활성화"
         name="login_screen_toggle"
         width="270"
         control_name="FSDisableLoginScreens"
         tool_tip="검은색 로그인 진행률 화면 비활성화"/>
        <check_box
         top_pad="3"
         follows="left|top"
         height="16"
         label="로그아웃 진행률 화면 비활성화"
         name="logout_screen_toggle"
         width="270"
         control_name="FSDisableLogoutScreens"
         tool_tip="검은색 로그아웃 진행률 화면을 비활성화합니다."/>

		<check_box
		 control_name="FSRenderFarClipStepping"
		 height="16"
		 label="점진적 드로잉 거리 스테핑"
		 layout="topleft"
		 name="FSRenderFarClipStepping"
		 tool_tip="활성화된 경우 Firestorm은 텔레포트 후 점진적인 드로잉 거리 스텝을 사용합니다."
		 initial_value="true"
		 top_pad="10"
		 width="250" />
		<slider
		 name="progressive_draw_distance_interval"
		 control_name="FSRenderFarClipSteppingInterval"
		 enabled_control="FSRenderFarClipStepping"
		 decimal_digits="0"
		 left="10"
		 top_pad="2"
		 follows="left|top"
		 layout="topleft"
		 height="18"
		 increment="1"
		 initial_value="20"
		 max_val="30"
		 min_val="5"
		 width="225"
		 tool_tip="각 그리기 거리 증가 사이의 간격(초)"/>

        <check_box
         top_pad="10"
         control_name="UseLSLBridge"
         follows="left|top"
         height="16"
         initial_value="true"
         name="UseLSLBridge"
         label="LSL-클라이언트 브리지 사용"
         left="10"
         width="270"
         tool_tip="뷰어가 스크립트로 작성된 첨부 파일(브릿지)을 사용하여 기능을 확장할 수 있습니다."/>
        <combo_box
         control_name="UseLSLFlightAssist"
         enabled_control="UseLSLBridge"
         layout="topleft"
		 left="10"
		 top_pad="2"
         follows="left|top"
         height="25"
         name="UseLSLFlightAssist"
         tool_tip="확장된 고도에서 비행 및 호버링을 지원하는 스크립트 사용"
         width="225">
            <combo_box.item
             label="비행 보조: 비활성화"
             name="flight_disabled"
             value="0.0" />
            <combo_box.item
             label="비행 보조: 마일드 부스트"
             name="flight_mild"
             value="0.5" />
            <combo_box.item
             label="비행 보조: 적당한 부스트"
             name="flight_moderate"
             value="1.0" />
            <combo_box.item
             label="비행 보조: 강한 부스트"
             name="flight_strong"
             value="2.0" />
            <combo_box.item
             label="비행 보조: 익스트림 부스트"
             name="flight_extreme"
             value="15.0" />
        </combo_box>
        <slider
         name="manual_environment_change_transition_period"
         control_name="FSEnvironmentManualTransitionTime"
         label="환경 전환 시간:"
         decimal_digits="1"
         left="12"
         top_pad="15"
         follows="left|top"
         layout="topleft"
         height="18"
         increment="0.5"
         initial_value="0"
         max_val="60"
         min_val="0"
         width="520"
         tool_tip="수동 환경 변경이 혼합되는 간격(초)입니다. 0이 인스턴스입니다."/>
        <check_box
         top_pad="5"
         control_name="EnvironmentPersistAcrossLogin"
         follows="left|top"
         height="16"
         initial_value="true"
         name="EnvironmentPersistAcrossLogin"
         label="전체 세션에서 지속적 환경 설정"
         left="10"
         width="500"
         tool_tip="다음 로그인 후 현재 환경 설정을 복원합니다."/>
    </panel>


    <!--Protection-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="보호"
     name="ProtectionTab">
        <check_box
         top_pad="10"
         left="10"
         label="개체에 앉아 있는 왼쪽 클릭 차단"
         control_name="FSBlockClickSit"
         follows="left|top"
         height="16"
         name="FSBlockClickSit"
         width="256" />
        <check_box
         top_pad="4"
         label="스크립트에 맵 UI 표시 허용 (llMapDestination)"
         control_name="ScriptsCanShowUI"
         follows="left|top"
         height="16"
         name="ScriptsCanShowUI"
         width="256" />

        <text
         top_pad="5"
         follows="left|top"
         height="12"
         name="revokepermissions_txt"
         width="200">
         사용 권한 취소:
         </text>
        <radio_group
         top_pad="5"
         left="20"
         height="62"
         layout="topleft"
         control_name="FSRevokePerms"
         name="FSRevokePerms"
         tool_tip=""
         width="331">
            <radio_item
             height="16"
             label="절대 (원래 행동)"
             layout="topleft"
             name="never_radio"
             value="0"
             top_pad="0"
             width="150" />
            <radio_item
             height="16"
             label="SIT에서 해지"
             layout="topleft"
             top_pad="0"
             name="sit_radio"
             value="1"
             width="150" />
            <radio_item
             height="16"
             label="STAND에서 해지"
             layout="topleft"
             top_pad="0"
             name="stand_radio"
             value="2"
             width="150" />
            <radio_item
             height="16"
             label="SIT 및 STAND에서 해지"
             layout="topleft"
             top_pad="0"
             name="sitnstand_radio"
             value="3"
             width="150" />
        </radio_group>

        <text
         left="10"
         top_pad="5"
         follows="left|top"
         height="12"
         name="SurfaceAreaThreshholdLabel"
         width="360">
         텍스처 지연 방지(일부 개체 손상 가능):
         </text>
        <check_box
         top_pad="8"
         left="20"
         label="텍스처 영역이 높은 큰 개체를 자동으로 숨깁니다. 임계값: "
         control_name="RenderVolumeSAProtection"
         follows="left|top"
         height="16"
         name="RenderVolumeSAProtection"
         width="380"
         tool_tip="이렇게 하면 뷰어를 손상시키도록 설계된 넓은 표면적에 질감이 매우 큰 객체로부터 사용자를 보호할 수 있습니다. 기본값은 5000제곱미터이지만 일부 개체가 렌더링되지 않고 이 옵션이 활성화된 경우 이 값을 늘려야 할 수 있습니다."/>
        <spinner
         enabled_control="RenderVolumeSAProtection"
         top_delta="-5"
         control_name="RenderVolumeSAFrameMax"
         decimal_digits="0"
         layout="topleft"
         follows="left|top"
         height="18"
         left_pad="5"
         increment="1"
         initial_value="5000"
         max_val="10000"
         min_val="500"
         name="RenderVolumeSAFrameMax"
         width="90" />

        <check_box
         top_pad="10"
         left="10"
         label="스팸 방지 사용"
         control_name="UseAntiSpam"
         follows="left|top"
         height="16"
         name="UseAntiSpam"
         width="200" />
        <!-- FS:TS FIRE-23138: Enable spam protection for user's objects -->
        <check_box
         left="20"
         label="사용자가 소유한 개체에 대해서도 스팸 방지 사용"
         control_name="FSUseAntiSpamMine"
         enabled_control="UseAntiSpam"
         follows="left|top"
         height="16"
         name="FSUseAntiSpamMine"
         width="380"
         tool_tip="이렇게 하면 소유한 개체에도 스팸 방지 보호 기능이 적용됩니다."/>
        <!-- /FS:TS FIRE-23138 -->
        <text
         top_pad="5"
         type="string"
         length="1"
         follows="left|top"
         height="15"
         layout="topleft"
         left="30"
         name="AntiSpamText1"
         width="200">
           단일 메시지에서 최대 줄 수:
        </text>
        <spinner
         enabled_control="UseAntiSpam"
         top_delta="-5"
         control_name="_NACL_AntiSpamNewlines"
         decimal_digits="0"
         layout="topleft"
         follows="left|top"
         height="18"
         left_pad="0"
         increment="1"
         initial_value="70"
         max_val="1000"
         min_val="5"
         name="_NACL_AntiSpamNewlines"
         width="50"
         tool_tip="단일 텍스트 메시지에서 수락할 최대 줄 수 [기본값: 70]"/>
        <text
         top_pad="10"
         type="string"
         length="1"
         follows="left|top"
         height="15"
         layout="topleft"
         left="30"
         name="AntiSpamText2"
         width="200">
           같은 소스에서 최대 이벤트:
        </text>
        <spinner
         enabled_control="UseAntiSpam"
         top_delta="-5"
         control_name="_NACL_AntiSpamAmount"
         decimal_digits="0"
         layout="topleft"
         follows="left|top"
         height="18"
         left_pad="0"
         increment="1"
         initial_value="10"
         max_val="40"
         min_val="2"
         name="_NACL_AntiSpamAmount"
         width="50"
         tool_tip="동일한 소스에서 2초 이내에 수락할 최대 유사 이벤트 수 [기본값: 10]"/>
        <text
         top_pad="10"
         type="string"
         length="1"
         follows="left|top"
         height="15"
         layout="topleft"
         left="30"
         name="AntiSpamText3"
         width="200">
           사운드 재생 요청 승수:
        </text>
        <spinner
         enabled_control="UseAntiSpam"
         top_delta="-5"
         control_name="_NACL_AntiSpamSoundMulti"
         decimal_digits="0"
         layout="topleft"
         follows="left|top"
         height="18"
         left_pad="0"
         increment="1"
         initial_value="10"
         max_val="25"
         min_val="1"
         name="_NACL_AntiSpamSoundMulti"
         width="50"
         tool_tip="사운드 재생 요청에 대한 이벤트 승수 [기본값: 10]"/>
        <text
         top_pad="10"
         type="string"
         length="1"
         follows="left|top"
         height="15"
         layout="topleft"
         left="30"
         name="AntiSpamText4"
         width="200">
           사운드 프리로드 요청 승수:
        </text>
        <spinner
         enabled_control="UseAntiSpam"
         top_delta="-5"
         control_name="_NACL_AntiSpamSoundPreloadMulti"
         decimal_digits="0"
         layout="topleft"
         follows="left|top"
         height="18"
         left_pad="0"
         increment="1"
         initial_value="4"
         max_val="25"
         min_val="1"
         name="_NACL_AntiSpamSoundPreloadMulti"
         width="50"
         tool_tip="사운드 프리로드 요청에 대한 이벤트 승수 [기본값: 4]"/>
        <button
         enabled_control="UseAntiSpam"
         layout="topleft"
         follows="left|top"
         top_pad="10"
         width="200"
         height="20"
         left="40"
         name="AntiSpamUnblock"
         label="모든 스팸 소스 차단 해제"
         commit_callback.function="NACL.AntiSpamUnblock"/>

        <check_box
         top_pad="8"
         left="10"
         label="결제하기 전에 확인하세요. 임계값:"
         control_name="FSConfirmPayments"
         follows="left|top"
         height="16"
         name="FSConfirmPayments"
         width="205"
         tool_tip="임계값보다 큰 금액을 지불하기 전에 확인 대화 상자를 표시합니다. 항상 확인을 표시하려면 임계값을 0으로 설정합니다."/>
        <spinner
         enabled_control="FSConfirmPayments"
         top_delta="-5"
         control_name="FSPaymentConfirmationThreshold"
         decimal_digits="0"
         layout="topleft"
         follows="left|top"
         height="18"
         left_pad="5"
         increment="1"
         initial_value="200"
         max_val="2147483647"
         min_val="0"
         name="FSPaymentConfirmationThreshold"
         tool_tip="임계값보다 큰 금액을 지불하기 전에 확인 대화 상자를 표시합니다. 항상 확인을 표시하려면 임계값을 0으로 설정합니다.."
         width="100"/>
    </panel>


    <!--Avatar-->
    <panel
     top_pad="5"
     bottom="-1"
     left="1"
     right="-1"
     follows="all"
     label="아바타"
     name="firestorm_avatar" >

        <text
         type="string"
         left="5"
         top_pad="12"
         length="1"
         follows="left|top"
         height="12"
         layout="topleft"
         name="HeadMovement"
         width="500">
            아바타의 머리가 마우스를 따라가는 양(움직이지 않을 경우 '0'으로 설정):
        </text>
        <slider
         control_name="PitchFromMousePosition"
         decimal_digits="0"
         follows="left|top"
         height="16"
         increment="15"

         height="16"
         label="임의 아바타 눈동자 이동 비활성화"
         left="15"
         name="FSStaticEyes"
         width="250"
         control_name="FSStaticEyes"/>

        <!-- LGG Color Beams -->
        <text
         name="BeamPrefs"
         follows="left|top"

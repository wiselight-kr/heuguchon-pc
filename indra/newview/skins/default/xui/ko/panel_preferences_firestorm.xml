         bottom_delta="25" >
            선택 빔 입자 효과
        </text>
        <view_border
         bevel_style="none"
         border_thickness="1"
         top_delta="15"
         follows="top|left"
         height="0"
         left="15"
         name="BeamDivisor"
         width="490" />

        <button
         enabled_control="ShowSelectionBeam"
         name="BeamColor_delete"
         label="삭제"
         tool_tip="이 프리셋 삭제"
         left="15"
         top_delta="10"
         width="55"
         height="20"
         follows="left|top" />

        <combo_box
         enabled_control="ShowSelectionBeam"
         allow_text_entry="false"
         top_delta="0"
         left_delta="60"
         follows="left|top"
         height="20"
         max_chars="20"
         name="BeamColor_combo"
         width="170"
         control_name="FSBeamColorFile"

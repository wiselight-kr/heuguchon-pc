<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
  follows="all"
  height="570"
  layout="topleft"
  name="item properties"
  help_topic="item_properties"
  title="아이템 프로필"
  width="333">
  <panel.string
	name="loading_experience">
    (로딩중)
  </panel.string>
  <panel.string
    name="unknown">
    (알 수 없음)
  </panel.string>
  <panel.string
    name="unknown_multiple">
    (알 수 없음 / 다중)
  </panel.string>
  <panel.string
    name="public">
    (공공의)
  </panel.string>
  <panel.string
    name="you_can">
    할 수 있음:
  </panel.string>
  <panel.string
    name="owner_can">
    소유자가 할 수 있음:
  </panel.string>
  <panel.string
    name="acquiredDate">
    [wkday,datetime,slt] [mth,datetime,slt] [day,datetime,slt] [hour,datetime,slt]:[min,datetime,slt]:[second,datetime,slt] [year,datetime,slt]
  </panel.string>
  <panel.string
    name="origin_inventory">
    (인벤토리)
  </panel.string>
  <panel.string
    name="origin_inworld">
    (인월드)
  </panel.string>
  <icon
    follows="top|right"
    height="18"
    image_name="Lock"
    layout="topleft"
    right="-15"
    mouse_opaque="true"
    name="IconLocked"
    top="8"
    width="18" />
  <button
    follows="top|left"
    height="24"
    image_hover_unselected="BackButton_Over"
    image_pressed="BackButton_Press"
    image_unselected="BackButton_Off"
    layout="topleft"
    left="12"
    name="back_btn"
    tab_stop="false"
    top="2"
    width="30"
    use_draw_context_alpha="false" />
  <text
    follows="top|left|right"
    font="SansSerifHugeBold"
    height="26"
    layout="topleft"
    left_pad="3"
    name="title"
    text_color="LtGray"
    top="2"
    use_ellipses="true"
    value="Item Profile"
    width="275" />
  <text
    follows="top|left|right"
    height="13"
    layout="topleft"
    left="45"
    name="origin"
    text_color="LtGray_50"
    use_ellipses="true"
    value="(인벤토리)"
    width="275" />
  <scroll_container
    color="SL-Background_66"
    follows="all"
    layout="topleft"
    left="9"
    name="item_profile_scroll"
    opaque="true"
    height="493"
    width="313"
    top="45">
    <panel
      follows="left|top|right"
      height="390"
      help_topic=""
      label=""
      layout="topleft"
      left="0"
      name="item_profile"
      top="0"
      width="295">
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemNameTitle"
        top="10"
        width="78">
        이름:
      </text>
      <line_editor
        border_style="line"
        border_thickness="1"
        follows="left|top|right"
        height="20"
        layout="topleft"
        left_delta="78"
        max_length_bytes="63"
        name="LabelItemName"
        top_delta="0"
        width="210"
        tool_tip="이름은 63자로 제한됩니다. 긴 원시 이름은 짧게 자릅니다. 이름은 수직 막대/파이프 &apos;|&apos;를 제외하고 ASCII-7(확장되지 않은) 문자 집합에 있는 인쇄 가능한 문자로만 구성될 수 있습니다." />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemDescTitle"
        top_pad="10"
        width="78">
        묘사:
      </text>
      <line_editor
        border_style="line"
        border_thickness="1"
        follows="left|top|right"
        height="23"
        layout="topleft"
        left_delta="78"
        max_length_bytes="127"
        name="LabelItemDesc"
        top_delta="-5"
        width="210"
        tool_tip="사람들이 &apos를 가질 때;뷰어 설정에서 선택한 모든 개체에 대한 마우스 포인터 아래에 개체 설명 팝업이 표시됩니다. Prim 설명은 잘릴 문자열보다 긴 문자열로 127바이트로 제한됩니다." />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="23"
        layout="topleft"
        left="5"
        name="LabelCreatorTitle"
        top_pad="10"
        width="78">
        창조자:
      </text>
      <avatar_icon
        follows="top|left"
        height="20"
        default_icon_name="Generic_Person"
        layout="topleft"
        left_pad="0"
        top_delta="-6"
        mouse_opaque="true"
        width="20" />
      <text
        type="string"
        follows="left|right|top"
        font="SansSerifSmall"
        height="15"
        layout="topleft"
        left_pad="5"
        name="LabelCreatorName"
        top_delta="6"
        use_ellipses="true"
        width="165">
      </text>
      <button
        follows="top|right"
        height="16"
        image_selected="Inspector_I"
        image_unselected="Inspector_I"
        layout="topleft"
        right="-5"
        name="BtnCreator"
        top_delta="-6"
        width="16" />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="23"
        layout="topleft"
        left="5"
        name="LabelOwnerTitle"
        top_pad="10"
        width="78">
        소유자:
      </text>
      <avatar_icon
        follows="top|left"
        height="20"
        default_icon_name="Generic_Person"
        layout="topleft"
        left_pad="0"
        top_delta="-6"
        mouse_opaque="true"
        width="20" />
      <text
        type="string"
        follows="left|right|top"
        font="SansSerifSmall"
        height="15"
        layout="topleft"
        left_pad="5"
        name="LabelOwnerName"
        top_delta="6"
        use_ellipses="true"
        width="165">
      </text>
      <button
        follows="top|right"
        height="16"
        image_selected="Inspector_I"
        image_unselected="Inspector_I"
        layout="topleft"
        right="-5"
        name="BtnOwner"
        top_delta="-3"
        width="16" />
      <text
        type="string"
        length="1"
        follows="left|top"
        height="23"
        layout="topleft"
        left="5"
        name="LabelAcquiredTitle"
        top_pad="10"
        width="78">
        획득:
      </text>
      <text
        type="string"
        length="1"
        follows="left|top|right"
        height="23"
        layout="topleft"
        left_delta="78"
        name="LabelAcquiredDate"
        top_delta="0"
        width="210">
      </text>
      <text
        type="string"
        length="1"
        follows="left|top"
        height="10"
        layout="topleft"
        left="5"
        name="LabelItemExperienceTitle"
        top_pad="0"
        width="78"
        visible="true">
        경험:
      </text>
      <text
        type="string"
        length="1"
        follows="left|top|right"
        height="10"
        layout="topleft"
        left_delta="78"
        name="LabelItemExperience"
        top_delta="0"
        width="210"
        visible="true"
        />
      <panel
        border="false"
        follows="left|top|right"
        layout="topleft"
        mouse_opaque="false"
        name="perms_inv"
        left="0"
        top_pad="25"
        height="155"
        width="313">
        <text
          type="string"
          length="1"
          left="10"
          top_pad="13"
          text_color="EmphasisColor"
          height="15"
          follows="left|top|right"
          layout="topleft"
          name="perm_modify"
          width="200">
          할 수 있음:
        </text>
        <check_box
          height="18"
          label="수정"
          layout="topleft"
          left="20"
          name="CheckOwnerModify"
          top_pad="0"
          width="90" />
        <check_box
          height="18"
          label="복사"
          layout="topleft"
          left_pad="0"
          name="CheckOwnerCopy"
          width="90" />
        <check_box
          height="18"
          label="이전"
          layout="topleft"
          left_pad="0"
          name="CheckOwnerTransfer"
          width="106" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="16"
          layout="topleft"
          left="10"
          name="AnyoneLabel"
          top_pad="8"
          width="100">
          누구든:
        </text>
        <check_box
          height="18"
          label="복사"
          layout="topleft"
          left_pad="0"
          name="CheckEveryoneCopy"
          tool_tip="누구나 그 물건의 복사본을 가져갈 수 있다. 개체 및 개체의 모든 내용은 복사 및 전송 허용이어야 합니다."
          top_delta="-2"
          width="150" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="16"
          layout="topleft"
          left="10"
          name="GroupLabel"
          top_pad="8"
          width="100">
          그룹:
        </text>
        <check_box
          height="18"
          label="공유"
          layout="topleft"
          left_pad="0"
          top_delta="-2"
          name="CheckShareWithGroup"
          tool_tip="집합 그룹의 모든 구성원이 이 개체에 대한 수정 권한을 공유할 수 있습니다. 역할 제한을 사용하려면 증서를 작성해야 합니다."
          width="150" />
        <text
          type="string"
          length="1"
          follows="left|top"
          height="16"
          layout="topleft"
          left="10"
          name="NextOwnerLabel"
          top_pad="8"
          width="200"
          word_wrap="true">
          다음 소유자:
        </text>
        <check_box
          height="18"
          label="수정"
          layout="topleft"
          left="20"
          top_pad="0"
          name="CheckNextOwnerModify"
          tool_tip="다음 소유자는 이 개체의 항목 이름 또는 축척과 같은 속성을 편집할 수 있습니다."
          width="90" />
        <check_box
          height="18"
          label="복사"
          layout="topleft"
          left_pad="0"
          name="CheckNextOwnerCopy"
          tool_tip="다음 소유자는 이 개체의 복사본을 무제한으로 만들 수 있습니다. 복사본은 작성자 정보를 유지하며, 복사되는 항목보다 더 허용적일 수 없습니다."
          width="90" />
        <check_box
          height="18"
          label="이전"
          layout="topleft"
          left_pad="0"
          name="CheckNextOwnerTransfer"
          tool_tip="다음 소유자는 이 개체를 양도하거나 재판매할 수 있습니다."
          width="106" />
      </panel>
      <check_box
        height="18"
        label="판매중"
        layout="topleft"
        left="20"
        name="CheckPurchase"
        top_pad="20"
        width="100"
        tool_tip="사람들이 지정된 가격으로 이 개체, 컨텐츠 또는 복사본을 세계에서 구입할 수 있습니다." />
      <combo_box
        height="23"
        left_pad="0"
        layout="topleft"
        follows="left|top"
        name="ComboBoxSaleType"
        width="170"
        tool_tip="구매자가 내용 또는 품목 자체의 사본, 사본 또는 사본을 받을지 선택." >
        <combo_box.item
             name="Copy"
             label="복사"
             value="2" />
        <combo_box.item
             name="Contents"
             label="내용"
             value="3" />
        <combo_box.item
             name="Original"
             label="원본"
             value="1" />
      </combo_box>
      <spinner
        follows="left|top"
        decimal_digits="0"
        increment="1"
        name="Edit Cost"
        label="가격: L$"
        label_width="75"
        left="120"
        width="170"
        min_val="0"
        height="23"
        max_val="999999999"
        top_pad="10"
        tool_tip="개체 가격." />
    </panel>

  </scroll_container>
  <panel
    height="30"
    layout="topleft"
    name="button_panel"
    left="5"
    top_pad="0"
    width="313"
    follows="top|right|left">
    <button
      follows="top|right"
      height="23"
      label="취소"
      layout="topleft"
      name="cancel_btn"
      right="-1"
      width="100" />
  </panel>
</panel>

<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
  layout="topleft"
  left="0"
  top="0"
  width="300"
  height="100"
  min_height="100"
  follows="all"
  border="true"
  name="experince_list_editor">
  <panel.string
    name="loading">
    로딩중...
  </panel.string>
   <panel.string
      name="panel_allowed">
      허락된 경험:
   </panel.string>
   <panel.string
      name="panel_blocked">
      차단된 경험:
   </panel.string>
   <panel.string
      name="panel_trusted">
      키 경험:
   </panel.string>
  <panel.string
    name="no_results">
    (비어있음)
  </panel.string>
   <text
      layout="topleft"
      name="text_name"
      left="3"
      right="120"
      height="12"
      follows="left|top">
      경험 목록
   </text>
   <text
      layout="topleft"
      name="text_count"
      left="125"
      top_pad="-12"
      right="-1"
      height="12"
      follows="top|left">
   </text>
  <name_list
    draw_heading="false"
    left="3"
    width="225"
    height="75"
    follows="all"
    name="experience_list">
    <name_list.columns
      user_resize="false"
      name="experience_name"
      label="이름"/>
  </name_list>
  <button
    layout="topleft"
    follows="top|right"
    top_pad="-77" 
    left_pad="3"
    width="65"
    name="btn_add"
    label="추가..."/>
  <button
    layout="topleft"
    follows="top|right"
    width="64"
    name="btn_remove"
    label="제거"/>
  <button
    layout="topleft"
    follows="top|right"
    width="64"
    name="btn_profile"
    label="프로필..."/>
</panel>

<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="Url Popup">
    <menu_item_call
     label="웹 페이지 열기"
     layout="topleft"
     name="url_open">
        <menu_item_call.on_click
         function="Url.Open" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft" />
    <menu_item_call
     label="내부 브라우저에서 열기"
     layout="topleft"
     name="url_open_internal">
        <menu_item_call.on_click
         function="Url.OpenInternal" />
    </menu_item_call>
    <menu_item_call
     label="외부 브라우저에서 열기"
     layout="topleft"
     name="url_open_external">
        <menu_item_call.on_click
         function="Url.OpenExternal" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft" />
    <menu_item_call
     label="클립보드에 URL 복사"
     layout="topleft"
     name="url_copy">
        <menu_item_call.on_click
         function="Url.CopyUrl" />
    </menu_item_call>
</context_menu>
